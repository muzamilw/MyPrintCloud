
<div id="Footer">
    <div class="spclColor">
        <div class="GreyBackS6">
            <div class="container heightauto content_area">
                <div class="row left_right_padding">
                    <div class="space5">
                        &nbsp;
                    </div>
                    <div class="footer_portion themeFontColor">
                        @foreach (var item in ViewData["PageCategory"] as List<MPC.Webstore.Models.PageCategory>)
                        {
                            <div class="footer_left_portion_division float_left_simple">
                                <label id="catName" class="heading_small_orange" style="font-size: 14px !important;font-weight:bolder !important;color:white !important;">@item.CategoryName</label>

                                @foreach (var page in ViewData["CmsPage"] as List<MPC.Webstore.Models.CmsPageModel>)
                                {
                                    if (page.CategoryId == item.CategoryId)
                                    {
                                        <ul>
                                            <li>
                                                <a href="#" id="secPages" class="uSecPageList" style="color:white !important;">@page.PageTitle</a>
                                            </li>
                                        </ul>
                                    }
                                }
                            </div>
                        }
                    </div>
                    <div class="space5">
                        &nbsp;
                    </div>
                    <div class="clearBoth">
                        &nbsp;
                    </div>
                </div>
                <div class="clearBoth">
                    &nbsp;
                </div>
            </div>
        </div>
    </div>
</div>
