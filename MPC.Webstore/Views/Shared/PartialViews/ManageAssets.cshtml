                                <a href="@RedirectUrl"><img src="@ParentFolder.ImagePath"></a>
                               }
                               else
                               {
                                <a href="@RedirectUrl">
                                    <img src="/Content/Images/imgPlc.png">
                                </a>
                                    }
</div>
                            <div class="assetNameCnt">
                                <a href="@RedirectUrl" class="catAssetName">@ParentFolder.FolderName</a>
                            </div>


                        </div>
                        @if (ContactRole != 3)
                        {
                            <div class="editBtnCnt">
                                <a onclick="ActionForFolder(@ParentFolder.FolderId,'@ParentFolder.ImagePath',this)" class="catButton icon basket"><span>Edit Folder</span></a>
                            </div>

