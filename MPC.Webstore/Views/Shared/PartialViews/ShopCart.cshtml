    function SendProofs()
    {
        var ModeOfStore = '@ViewBag.StoreMode';

        var email1 = $("#Email1").val();
        var email2 = $("#Email2").val();

        var orderID ='@ViewBag.OrderId';
        var ContactID = '@ViewBag.ContactID';
        var ComapanyID ='@ViewBag.StoreId';

        if(EmailValidation(email1,email2)==true)
        {

            $.ajax({
                type:"POST",
                url:"/webstoreapi/ContactImageUploader/SendEmails?orderID="+orderID+"&Email1="+email1+"&Email2="+email2+"&ContactID="+ContactID+"&ContactCompanyID="+ComapanyID+"&StoreModee="+ModeOfStore+"",
                contentType: "application/json",
                //data: data,
                async: true,
                success: function (response) {
                    $("#proofMesges").css("display", "block");
                    $("#proofMesges").html("Email has been sent to the below email address(es)");
                },
                error: function(reponse){
                    alert(reponse);
                }
            });
        }


    }
    function EmailValidation(email1,email2)
    {

        $("#Email1").css("border", "1px solid #c1c6cc");
        $("#Email2").css("border", "1px solid #c1c6cc");

        var Result=false;
        if (email1 != "") {

            if (ValidateEmail(email1)==false) {

                $("#Email1").css("border", "2px solid red");
                $("#proofMesges").css("display", "block");
                $("#proofMesges").html("Please enter a valid email address in address 1");
            }
            else
            {
                Result=true;
                $("#Email1").css("border", "1px solid #c1c6cc");
            }
        }
        if (email2 != "") {
            if (ValidateEmail(email2)==false) {
                $("#proofMesges").css("display", "block");
                $("#proofMesges").html("<br/>Please enter a valid email address in address 2");
              
                $("#Email2").css("border", "2px solid red");
            }
            else
            {
                Result=true;
                $("#Email2").css("border", "1px solid #c1c6cc");
            }
        }
        if (email2 == "" && email1 == "") {
            //alert("Please enter atleast one email address");
            Result=false;
            $("#Email1").css("border", "2px solid red");
            $("#Email2").css("border", "2px solid red");
        }

        return Result;
    }
    function ValidateEmail(email) {
        var isValid = true;
        if (email == '') {

            isValid = false;
        }
        else {
            var re = new RegExp("^[A-Za-z0-9](([_\\.\\-]?[a-zA-Z0-9]+)*)@@([A-Za-z0-9]+)(([\\.\\-]?[a-zA-Z0-9]+)*)\\.([A-Za-z]{2,})$");
            if (!re.test(email)) {

                isValid = false;
            }
        }
        return isValid;
    }


    function CanCheckOut(sender) {
        if (Page_ClientValidate()) {
            var numOfRec = 0;
            if("@ViewBag.NumberOfRecords" != ""){
                numOfRec = "@ViewBag.NumberOfRecords";
            }


            if (parseInt(numOfRec) > 0) {
                $("hfIsCallFrom").val("CheckOut");
                return true;
            }
            else {
                ShowPopup('@Html.Raw(Html.GetKeyValueFromResourceFile("ltrlMessage", UserCookieManager.WBStoreId, "Message"))','@Html.Raw(Html.GetKeyValueFromResourceFile("ltrlnoitmstocheckout", UserCookieManager.WBStoreId, "No item(s) to checkout"))');
                return false;
            }
        } else {
            return false;
        }
    }

    

    function EmailItems() {
        var ModeOfStore = '<%=Mode %>';
        var BrokerCompanyId = '<%=BrokerCompanyID %>';
        var email1 = $("#userEmail1").val();
        var email2 = $("#userEmail2").val();
        var orderID = $('#<%=hfOrderID.ClientID %>').val();
        var ContactID = $('#<%=hfContactID.ClientID %>').val();
        var ComapanyID = $('#<%=hfCompanyID.ClientID %>').val();
        var filter = /^([\w-\.]+)((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
        if (email1 != "") {
            if (!filter.test(email1)) {
                alert('@Html.Raw(Html.GetKeyValueFromResourceFile("ltrlvalidemailaddress1", UserCookieManager.WBStoreId, "Are you sure you want to remove this item from your cart?"))');
                return false;
            }
        }
        if (email2 != "") {
            if (!filter.test(email2)) {
                alert('@Html.Raw(Html.GetKeyValueFromResourceFile("ltrlvalidemailaddress2", UserCookieManager.WBStoreId, "Please enter a valid email address in address 2"))');
                return false;
            }
        }
        if (email2 == "" && email1 == "") {
            alert('@Html.Raw(Html.GetKeyValueFromResourceFile("ltrlenteratleast1email", UserCookieManager.WBStoreId, "Please enter atleast one email address"))');
            return false;
        }
        $.get("Services/Webstore.svc/sendProofs", { orderID: orderID, Email1: email1, Email2: email2, ContactID: ContactID, ContactCompanyID: ComapanyID, StoreMode: ModeOfStore, BrokerContactCompanyID: BrokerCompanyId },
            function (data) {
                //alert(data);
                if (data == true) {
                    $(".proofSuccess").css("display", "block");
                }
                //window.location.href = data + "&CategoryId=" + CategoryId + "&ProductName=" + document.getElementById('txtDesignName').value + emailParameters;
            });
        return false;
    }
