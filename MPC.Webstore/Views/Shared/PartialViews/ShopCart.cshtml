                                                    <span id="lblItemProductName" class="heading_16">
                                                        @if (!string.IsNullOrEmpty(prod.ProductFriendlyName))
                                                        {
                                                            @prod.ProductFriendlyName
                                                        }
                                                        else
                                                        {
                                                            @prod.ProductName
                                                        }
                                                    </span><span id="CopyTxt" class="heading_16" style="color: red !important;"></span>
                                                    <br>
                                                    @if (!string.IsNullOrEmpty(prod.ProductCategoryName))
                                                    {
                                                        <span id="lblProductCategory" class="ItemdetailDesc">( @prod.ProductCategoryName )</span>
                                                        <br />
                                                    }
                                                
                                                    <br>
                                                    @if (prod.IsDigitalDownloadOrdered != true)
                                                    {
                                                        <span id="lblSlectedStockName" class="ItemdetailDesc">@prod.PaperType</span>
                                                        <br>
                                                    }
                                                    

                                                    @{
                                                        string itemAddsOn = string.Empty;
                                                        ProductProductionTime = prod.EstimateProductionTime ?? 0;
                                                        if (ViewData["selectedItemsAddonsList"] != null)
                                                        {
                                                            List<AddOnCostsCenter> costcenters = ViewData["selectedItemsAddonsList"] as List<AddOnCostsCenter>;
                                                            foreach (AddOnCostsCenter cost in costcenters.Where(addon => addon.ItemID == prod.ItemID))
                                                            {
                                                                itemAddsOn = itemAddsOn + cost.AddOnName + "<br />" + cost.CostCentreDescription.Replace("---", "<br />");
                                                                //noOfCostCentreProdDays += cost.EstimateProductionTime;
                                                            }
                                                        }

                                                        //ProductProductionTime += noOfCostCentreProdDays;
                                                        //if (_numOfProductionDays < ProductProductionTime)
                                                        //{
                                                        //    _numOfProductionDays = ProductProductionTime;
                                                        //}
                                                        _numOfProductionDays = Model.TotalProductionTime;

                                                        DateTime date = Utils.AddBusinessdays(_numOfProductionDays, DateTime.Now);

                                                        EstimateDispatchTime = date.ToString("MMMM dd, yyyy");
                                                    }
                                                    @if (prod.IsDigitalDownloadOrdered != true)
                                                    {
                                                        <span id="lblselectedAddOnList" class="ItemdetailDesc">@Html.Raw(itemAddsOn)</span>
                                                    }
                                                    
                                                    <input type="hidden" id="numberOfDaysAddedTodelivery" name="numberOfDaysAddedTodelivery" value="@numberOfDaysAddedTodelivery">
                                                    <br>
                                                    <input type="submit" id="linkRemoveCartItem" class="silver_back_button_remove removeBtnForeColor" onclick=" return ConfirmDeleteItem('@prod.ItemID', '@ViewBag.OrderID', event); " value="@Html.Raw(Html.GetKeyValueFromResourceFile("ltrremoveproduct", UserCookieManager.WBStoreId, "Remove product from cart"))">
                                                    <br />
                                                    @if (prod.ProductType != (int) ProductType.AssetProduct)
                                                    {
                                                        <input type="submit" id="btnCopyProduct" class="silver_back_button" onclick=" CopyProduct('@prod.ItemID', event); " value="@Html.Raw(Html.GetKeyValueFromResourceFile("ltrlcopyproduct", UserCookieManager.WBStoreId, "Make a copy of this product in cart"))" />
                                                    }
                                                </td>
                                                <td class="PadTop5" align="left" style="width: 5%;" valign="top">
                                                    <span id="lblItemQuantity" class="fntQtyPrice" style="font-weight: bold;">@prod.Qty1</span>
                                                </td>
                                                @if (ViewBag.IsShowPrices == true)
                                                {

                                                    _vatTotal += prod.Qty1Tax1Value ?? 0;

                                                    double ItemActulVal = 0;

                                                    _priceTotal += prod.Qty1BaseCharge1 ?? 0;

                                                    _DiscountAmountTotal += prod.DiscountedAmount ?? 0;

                                                    if (prod.DiscountedAmount != null)
                                                    {
                                                        ItemActulVal = prod.Qty1BaseCharge1 + prod.DiscountedAmount ?? 0;
                                                    }

                                                    <td class="PadTop5 priceth" valign="top" align="right" style="width: 20%; padding-right: 0px;">

                                                        @if (prod.DiscountedVoucherId != null)
                                                        {
                                                            <span id="spnItemActualPrice" class="fntQtyPrice strikeThrough" style="font-weight: bold; text-align: right !important;">@Utils.FormatDecimalValueToTwoDecimal((ItemActulVal).ToString(), ViewBag.Currency)</span>
                                                            <span id="lblItemPrice" class="fntQtyPrice" style="font-weight: bold; text-align: right !important;">@Utils.FormatDecimalValueToTwoDecimal((prod.Qty1BaseCharge1 ?? 0).ToString(), ViewBag.Currency)</span>
                                                        }
                                                        else
                                                        {
                                                            <span id="lblItemPrice" class="fntQtyPrice" style="font-weight: bold; text-align: right !important;">@Utils.FormatDecimalValueToTwoDecimal((prod.Qty1BaseCharge1 ?? 0).ToString(), ViewBag.Currency)</span>
                                                        }

                                                    
                                                    </td>
                                                }


                                            </tr >

                                            subtotal = _priceTotal.ToString();
                                        }


                                    }
                                    @{

                                     _vatTotal = Convert.ToDouble(_vatTotal) + Convert.ToDouble(Model.DeliveryTaxValue);
                                     GrossTotal = Convert.ToDouble(subtotal) + Convert.ToDouble(_vatTotal) + Model.DeliveryCost;
                                     subtotal = Utils.FormatDecimalValueToTwoDecimal(subtotal.ToString(), ViewBag.Currency);
                                     VatTotal = Utils.FormatDecimalValueToTwoDecimal(_vatTotal.ToString(), ViewBag.Currency);
                                    }

                                </tbody>
                            </table>

                            <br />
                            <div class="cnttotalPayables">
                                @if (ViewBag.IsShowPrices)
                                {
                                    
                                    <div class="cntsubHeadings">
                                        <label  class=" LightGrayLabels displayClear">@Html.Raw(Html.GetKeyValueFromResourceFile("ltrlsubtotal", UserCookieManager.WBStoreId))</label>
                                        @if (_DiscountAmountTotal > 0)
                                        {
                                            <label id="lblVoucherDiscPercentageDisplay" class=" LightGrayLabels displayClear clrRed">@Html.Raw(Html.GetKeyValueFromResourceFile("lblVoucherDiscPercentageDisplay", UserCookieManager.WBStoreId, "Discount")):</label>

                                        }
                                        @if (ViewBag.IsDigitalDownloadOrder != true)
                                        {
                                            <label class=" LightGrayLabels displayClear">@Html.Raw(Html.GetKeyValueFromResourceFile("tddelivery", UserCookieManager.WBStoreId, "Delivery:"))</label>
                                        }
                                        
                                        <label class=" LightGrayLabels displayClear">@Model.TaxLabel</label>
                                        <label class=" LightGrayLabels displayClear">@Html.Raw(Html.GetKeyValueFromResourceFile("spntotal", UserCookieManager.WBStoreId, "Total:"))</label>
                                    </div>
                                    <div class="cnttotals" id="cntRightPricing1">
                                        <label  class="Fsize13 colorBlack displayClear">@subtotal</label>
                                        @if (_DiscountAmountTotal > 0)
                                        {
                                            <label id="lblVoucherCodeDiscAmount_shopCart" class="Fsize13 colorBlack displayClear clrRed">@Utils.FormatDecimalValueToTwoDecimal(_DiscountAmountTotal.ToString(), ViewBag.Currency)</label>
                                        }
                                        @if (ViewBag.IsDigitalDownloadOrder != true)
                                        {
                                            if (Model.DeliveryCostCenterID == 0)
                                            {
                                                if (Model.DeliveryDiscountVoucherID == null)
                                                {
                                                    <label class="Fsize13 colorBlack displayClear delivery-toBeCaltext" id="lblDeliveryToBeCal">@Html.Raw(Html.GetKeyValueFromResourceFile("ltrlTobeCalculated", UserCookieManager.WBStoreId, "To be calculated"))</label>
                                                }
                                                else
                                                {
                                                    <label class="Fsize13 colorBlack displayClear delivery-codetext" id="lblDeliveryToBeCal">@Html.Raw(Html.GetKeyValueFromResourceFile("freeeshippping", UserCookieManager.WBStoreId, "Free Shipping"))</label>
                                                }


                                            }
                                            else
                                            {
                                                if (Model.DeliveryDiscountVoucherID == null)
                                                {
                                                    <label class="Fsize13 colorBlack displayClear" id="lblCalculatedDelivery">@Utils.FormatDecimalValueToTwoDecimal(Convert.ToString(Model.DeliveryCost), ViewBag.Currency)</label>
                                                }
                                                else
                                                {
                                                    <label class="Fsize13 colorBlack displayClear delivery-codetext" id="lblCalculatedDelivery">@Html.Raw(Html.GetKeyValueFromResourceFile("freeeshippping", UserCookieManager.WBStoreId, "Free Shipping"))</label>


                                                }
                                            }
                                        }
                                        
                                        
                                        <label class="Fsize13 colorBlack displayClear">@VatTotal</label>
                                        <label class="Fsize13 colorBlack displayClear">@Utils.FormatDecimalValueToTwoDecimal(GrossTotal.ToString(), ViewBag.Currency)</label>
                                    </div>
                                }
                                <div class="clearBoth">
                                </div>
                            </div>
                            <div class="cntdispatchDelivery">
                                <div class="tdDispatchDate">
                                    @if (ViewBag.IsDigitalDownloadOrder != true)
                                        {
                                    <div class="estimated_dispactch_sec MLeft17 LightGrayLabels">
                                        
                                        <label id="lbl_EstimatedDispatch">@Html.Raw(Html.GetKeyValueFromResourceFile("spnestimateddispatchdate", UserCookieManager.WBStoreId, "Estimated dispatch date:"))</label>
                                        
                                        
                                        
                                    </div>
                                            <div class="estimated_dispatch_time MLeft17">
                                                <label id="lblDeliveryCostCenterDateText" class="heading_h5 colorBlack fontSyleBold">@EstimateDispatchTime</label>  &nbsp;
                                            </div>
                                        }
                                </div>
                                <div class="tdDelivery">
                                    <div class="estimated_dilvery_sec  LightGrayLabels" style="display:none;">
                                        &nbsp;
                                    </div>
                                    @if (ViewBag.DisableCouponCode == 0)
                                    {
                                        <div class="dropdown_sec">

                                            <div id="panelCouponCode">
                                                @if (ViewBag.VoucherApplied == null)
                                                {

                                                    <input type="text" class="txtCouponCode rounded_corners5" id="txtDiscountVoucherCode" placeholder="@Html.Raw(Html.GetKeyValueFromResourceFile("ltrlcoponcode",UserCookieManager.WBStoreId, "Coupon Code"))" />
                                                    <button id="btnApplyDiscountVoucherCode" type="button" class="btn_blue_back_small rounded_corners5" onclick="return ApplyDiscountVouchers();">@Html.Raw(Html.GetKeyValueFromResourceFile("btnAutoPostBackDiscountVoucherCode", UserCookieManager.WBStoreId, ""))</button>
                                                    <div id="lblRedeemErrorMesg" class="errorMessage"></div>
                                                    <button id="btnRemoveDeliveryVoucherCode" type="submit" class="btn_blue_back_small_Remove rounded_corners5" onclick="RemoveDeliveryVoucherCode(event);" style="display:none;">Remove voucher code</button>
                                                }
                                                else
                                                {
                                                    <button id="btnRemoveVoucherCode" type="submit" class="btn_blue_back_small_Remove rounded_corners5" onclick="RemoveVoucherCode(event);">@Html.Raw(Html.GetKeyValueFromResourceFile("ltrlremovevouchercode", UserCookieManager.WBStoreId, "Remove voucher code"))</button>
                                                }
                                            </div>

                                        </div>
                                    }
                                
                                </div>
                            </div>
                            <div class="clearBoth">
                            </div>
                            @*@if (ViewBag.CanPlaceOrder == 1)
                            {*@
                                <input type="submit" id="btnCheckout" value="CHECKOUT" class="start_creating_btn_checkout float_right  rounded_corners5" onclick="return ifNoAnEnterKay(event);" /><br />
                            @*}
                            else
                            {
                                <input type="button" id="btnCheckout" value="CHECKOUT" class="start_creating_btn_checkout float_right  rounded_corners5" onclick="CantPlaceOrder();" /><br />
                            }*@
                            @if (ViewBag.EmailProofButtonVisible == true)
                            {
                             
                                    <input id="prof_btn" style="margin-bottom:10px;" type="button" onclick="return EmailProofClick()" class="btn_brown float_right rounded_corners5" value="Send Email Proof" />
                               
                              
                            }
                           <a href="/">
                               <button id="btn_ContinueShopping" type="button" onclick="return BackToShopping()" class="btn_brown  rounded_corners5" style="float: right;">@Html.Raw(Html.GetKeyValueFromResourceFile("btnContinueShopping1", UserCookieManager.WBStoreId))</button>
                           </a> 
                            <div class="clearBoth">
                                &nbsp;
                            </div>
                        </div>
                         
                    </div>
                    <div class="clearBoth">
                        &nbsp;
                    </div>
                    <input type="hidden" id="hfIsCallFrom" name="hfIsCallFrom" value="Checkout" />
                    <input type="hidden" id="hfItemID" name="hfItemID" value="0" />
                    <input type="hidden" id="hfFreeShippingVoucherId" value="0" />
                    <input type="hidden" id="hfOrderID" name="hfOrderID" value="@ViewBag.OrderID" />
                    <input type="hidden" id="hfGrandTotal" name="hfGrandTotal" value="@ViewBag.GrossTotal" />

                }
            }


        }
        else
        {
            <div class="get_in_touch_box rounded_corners">
                <div class="textAlignCenter">
                    <label class="lbl_Empty_Cart">@Html.Raw(Html.GetKeyValueFromResourceFile("lblMsg", UserCookieManager.WBStoreId))</label>

                </div>

            </div>
        }

    </div>
    <div class="clearBoth">

    </div>
    @*matching set*@

    @{
        if (UserCookieManager.WEBStoreMode != (int)StoreMode.Corp)
        {

            // call matching set partial view to show matching sets
            MatchingSetViewModel MSViewModel = new MatchingSetViewModel();

            MSViewModel = ViewData["MSViewModel"] as MatchingSetViewModel;
            if (MSViewModel != null)
            {
                @Html.Partial("PartialViews/MatchingSet", MSViewModel);
            }


            // call related items to show lists of related items
            RelatedItemViewModel RIViewModel = new RelatedItemViewModel();
            RIViewModel = ViewData["RIViewModel"] as RelatedItemViewModel;
            if (RIViewModel != null)
            {
                @Html.Partial("PartialViews/RelatedItems", RIViewModel);
            }
            else
            {
                <label id="lbl_NorelatedItemsFound" class="errorMsgClss">@Html.Raw(Html.GetKeyValueFromResourceFile("lblNorelatedItemsFound", UserCookieManager.WBStoreId, "No Related Items Found"))</label>
            }
        }

    }
    <div class="clearBoth">

    </div>
</div>

<div class="opaqueLayer">
</div>
