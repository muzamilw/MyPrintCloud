                                                    <input type="hidden" id="numberOfDaysAddedTodelivery" name="numberOfDaysAddedTodelivery" value="@numberOfDaysAddedTodelivery">
                                                    <br>
                                                    <br>
                                                </td>
                                                <td class="PadTop5" align="left" style="width:5%;">
                                                    <span id="lblItemQuantity" class="fntQtyPrice" style="font-weight:bold;">@prod.Qty1</span>
                                                </td>
                                                @if (ViewBag.IsShowPrices)
                                                {

                                                    string itemPrice = string.Empty;
                                                    _vatTotal += prod.Qty1Tax1Value ?? 0;
                                                     
                                                    //2.Set item Price Now
                                                    double ItemActulVal = 0;
                                                
                                                    _priceTotal += prod.Qty1BaseCharge1 ?? 0;
                                                    _DiscountAmountTotal += prod.DiscountedAmount ?? 0;

                                                    if (prod.DiscountedAmount != null)
                                                    {
                                                        ItemActulVal = prod.Qty1BaseCharge1 + prod.DiscountedAmount ?? 0;
                                                    }
                                                    itemPrice = Utils.FormatDecimalValueToTwoDecimal((prod.Qty1BaseCharge1 ?? 0).ToString(), ViewBag.Currency);
                                                  
                                                    <td class="PadTop5 priceth" align="right" style="width:20%; padding-right:0px;">
                                                        @if (prod.DiscountedVoucherId != null)
                                                        {
                                                            <span id="spnItemActualPrice" class="fntQtyPrice strikeThrough" style="font-weight:bold;text-align: right !important;">@Utils.FormatDecimalValueToTwoDecimal((ItemActulVal).ToString(), ViewBag.Currency)</span>
                                                            <span id="lblItemPrice" class="fntQtyPrice" style="font-weight:bold;text-align: right !important;">@itemPrice</span>
                                                        }
                                                        else
                                                        {
                                                            <span id="lblItemPrice" class="fntQtyPrice" style="font-weight:bold;text-align: right !important;">@itemPrice</span>
                                                        }

                                                        
                                                    </td>
                                                }

                                            </tr>

                                                subtotal = Utils.FormatDecimalValueToTwoDecimal(_priceTotal.ToString(), ViewBag.Currency);
                                                double TotalTax = _vatTotal + Model.DeliveryTaxValue;
                                                VatTotal = Utils.FormatDecimalValueToTwoDecimal(TotalTax.ToString(), ViewBag.Currency);
                                                GrossTotal = _priceTotal + TotalTax + Model.DeliveryCost;

                                         }

                                        }
                                    </tbody>
                                </table>
                                <br />
                                @if (ViewBag.IsShowPrices)
                                {
                                    <div id="PnalTotalBox">
                                        <table border="0" cellpadding="3" cellspacing="0" width="100%" class="left_align">
                                            <tr>
                                                <td class="OCnfmtd1">
                                                    &nbsp;
                                                </td>
                                                <td width="25%">
                                                    &nbsp;
                                                </td>
                                                <td width="30%" align="left" class="fontWeightNormal Fsize13" id="td1" style="text-align:right;">
                                                    <span id="spnsubtotal" class="CartFonts colorBlack">@Html.Raw(Html.GetKeyValueFromResourceFile("ltrlsubtotal", UserCookieManager.WBStoreId, "Sub Total")):</span>
                                                </td>
                                                <td align="right" class="OCnfmtd4">
                                                    <label id="lblSubTotal" class="Fsize15 colorBlack">@subtotal</label>
                                                </td>
                                            </tr>
                                            @if (_DiscountAmountTotal > 0)
                                            {
                                                <tr>
                                                    <td class="OCnfmtd1" align="left">
                                                        &nbsp;
                                                    </td>
                                                    <td width="25%">
                                                        &nbsp;
                                                    </td>
                                                    <td width="30%" align="left" class="fontWeightNormal Fsize13" id="td2" style="text-align:right;">
                                                        <span id="spndiscount" class="CartFonts clrRed">@Html.Raw(Html.GetKeyValueFromResourceFile("lblVoucherDiscPercentageDisplay", UserCookieManager.WBStoreId, "Discount")):</span>
                                                    </td>
                                                    <td align="right"  class="OCnfmtd4">
                                                        <label id="lbldiscount" class="Fsize15 clrRed">@Utils.FormatDecimalValueToTwoDecimal((_DiscountAmountTotal).ToString(), ViewBag.Currency)</label>
                                                    </td>
                                                </tr>
                                            }
                                            <tr>
                                                <td align="left" class="OCnfmtd1">
                                                    &nbsp;
                                                </td>
                                                <td width="25%">
                                                    &nbsp;
                                                </td>
                                                <td width="30%" align="left" class="fontWeightNormal Fsize13" id="td2" style="text-align:right;">
                                                    <span id="spndelivery" class="CartFonts colorBlack">@Html.Raw(Html.GetKeyValueFromResourceFile("ltrlDelivery", UserCookieManager.WBStoreId, "Delivery")):</span>
                                                </td>
                                                <td align="right" class="OCnfmtd4">
                                                    @if (Model.DeliveryDiscountVoucherID == null)
                                                    {
                                                        <label id="lblDeliveryCostCenter" class="Fsize15 colorBlack">@Utils.FormatDecimalValueToTwoDecimal(Convert.ToString(Model.DeliveryCost), ViewBag.Currency)</label>
                                                    }
