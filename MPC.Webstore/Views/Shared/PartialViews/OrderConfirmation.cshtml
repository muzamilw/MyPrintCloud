                                                        <label class="Fsize13 colorBlack displayClear" style="color:red;">&nbsp;</label>
                                                    }

                                                </td>
                                            </tr>
                                            <tr id="Tr2">
                                                <td align="left" class="OCnfmtd1"></td>
                                                <td width="25%"></td>
                                                <td width="30%" align="left" class="fontWeightNormal Fsize13" style="text-align:right;">
                                                    <span id="lblTaxLabel" class="CartFonts colorBlack">@ViewBag.TaxLabel:</span>
                                                </td>
                                                <td align="right" class="OCnfmtd4">
                                                    <label id="lblVatTotal" class="Fsize15 colorBlack">@VatTotal</label>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td class="OCnfmtd1">
                                                    &nbsp;
                                                </td>
                                                <td width="25%">
                                                    &nbsp;
                                                </td>
                                                <td width="30%" align="left" class="fontWeightNormal Fsize13" id="td3" style="text-align:right;">
                                                    <span class="CartFonts colorBlack" id="spngrandtotal">@Html.Raw(Html.GetKeyValueFromResourceFile("lblTotalHeadingText", UserCookieManager.WBStoreId, "Total"))
:</span>
                                                </td>
                                                <td align="right" class="OCnfmtd4">
                                                    <label class="CartFonts colorBlack Fsize15" id="lblGrandTotal">@Utils.FormatDecimalValueToTwoDecimal(GrossTotal.ToString(), ViewBag.Currency)</label>
                                                </td>
                                            </tr>
                                        </table>
                                    </div>
                                }
                            </div>
                        </div>
