

<head>
    <script src="/Scripts/zoomSlider.js" type="text/javascript"></script>
    <link href="@Href("~/Content/zoomslider.css")" rel="stylesheet" type="text/css" />
</head>
@model List<MPC.Models.DomainModels.CompanyBanner>
@using MPC.Webstore.Common;
<div class="content_area">
    <div id="wrapper">
        <div id="zoom-slider">
            @foreach (var banner in Model)
            {
                <canvas width="1100" height="550" style="position: absolute; z-index: 1; display: none; opacity: 0; bottom: auto; top: -64px; right: auto; left: -129px; width: 1100px; height: 550px;"></canvas>
                <a id="hlItemUrl" href="@banner.ButtonURL">
                    <img id="imgBanner" src="@banner.ImageURL" />
                </a>
            }
        </div>
    </div>
</div>
