<div class="row" style="display: none" data-bind="visible: $root.isEditorVisible()">
    <div class="row">
        <div class="col-lg-12">
            <div class="row">
                <div class="col-lg-12" id="topArea" style="padding-left:16px;">
                    <div class="clearfix">
                        <h1 class="pull-left" style="padding-left:0px;margin-top:35px;">&nbsp;</h1>
                    </div>

                    <div class="col-lg-12" id="filter" style="height:50px;margin-bottom:7px;">
                        <div class="col-lg-2 pull-right" style="padding-right: 2%; ">
                            @*<div class="pull-right">
                                    <a class="btn btn-primary patial-edit">
                                        Add Delivery Note
                                    </a>
                                </div>*@
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row" id="prefixesdata" style="float:right; padding-right: 20px;">
        <div class="input-group">

            <button data-bind="click:$root.onCloseEditor" class="btn  btn-info btn-primary" style="float: right;margin-bottom: 10px;margin-right: 15px;display:inline;" id="closeBtn"><i class="fa fa-times fa-lg"></i>&nbsp;Close</button>
            <button class="btn  btn-info btn-primary" style="float: right;margin-bottom: 10px;margin-right: 15px;display:inline;" id="saveBtn">
                <i class="fa fa-floppy-o fa-lg"></i>&nbsp;Save
            </button>

            <button class="btn  btn-info btn-primary" style="float: right;margin-bottom: 10px;margin-right: 15px;display:inline;" id="saveBtn"><i class="fa fa-minus-circle fa-lg"></i>&nbsp;Delete</button>
            <button class="btn  btn-info btn-primary" style="float: right;margin-bottom: 10px;margin-right: 15px;display:inline;" id="saveBtn"><i class="fa fa-print fa-lg"></i>&nbsp;Print</button>
            <button class="btn  btn-info btn-primary" style="float: right;margin-bottom: 10px;margin-right: 15px;display:inline;" id="saveBtn"><i class="fa fa-envelope fa-lg"></i>&nbsp;Post</button>

        </div>
    </div>
    <div class="row" id="user-profile" data-bind="with: selectedDeliveryNote" style="padding-left: 24px; padding-right: 24px;">
        <div class="col-lg-12 col-md-8 col-sm-8">
            <div class="main-box clearfix">
                <div class="col-lg-12 form-horizontal" style="margin-top: 3%;">
                    <h3 class="group-header">General Information</h3>
                    <div class="col-lg-6">
                        <div class="form-group">
                            <div class="col-lg-4 text-right">
                                <div class="bs-example-tooltips tooltip-float-width">

                                </div>

                                <label class="control-label tooltip-float">&nbsp;</label>
                            </div>
                            <div class="col-lg-6">
                                <input type="text" style="height: 60px;" data-bind="value: companyName, validationOnElement: companyId, click: !$root.selectedDeliveryNote().deliveryNoteId() ? $root.openCompanyDialog : undefined" readonly placeholder="Select Customer" class="form-control font24px" id="txtCompany" />
                            </div>
                            <div class="col-sm-1" style="padding-top:10px;margin-left: 10px">
                                <button data-bind="click: $root.openCompanyDialog" class="btn btn-primary pull-right">
                                    <i class="fa fa-user"></i>
                                </button>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-4 text-right">
                                &nbsp;
                            </div>
                            <div class="col-sm-6" data-bind="if: $root.isCompanyBaseDataLoaded">
                                <select class="form-control" data-bind="options: $root.companyContacts, optionsText: 'name', optionsValue: 'id', optionsCaption: 'Select a contact', value: contactId"></select>
                                <span class="help-block text-right"></span>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-4 text-right">
                                &nbsp;
                            </div>
                            <div class="col-sm-6" data-bind="if: $root.isCompanyBaseDataLoaded">
                                <select class="form-control" data-bind="options: $root.companyAddresses, optionsText: 'name', optionsValue: 'id', optionsCaption: 'Select an address', value: addressId"></select>
                                <span class="help-block text-right"></span>
                            </div>
                            <div class="col-sm-1" data-bind="if: $root.isCompanyBaseDataLoaded" style="margin-left: 10px">
                                <a class="btn btn-primary pull-right" id="addresspull" onclick="addAddress()">
                                    <i id="addresspullicon" class="fa fa-chevron-circle-right drop-icon"></i>
                                </a>
                            </div>
                        </div>
                        <div id="popoveraddress" style="display:none;">
                            <!-- ko if: $root.selectedAddress()!==undefined && $root.selectedAddress().address1!=='' && $root.selectedAddress().address1!== undefined-->
                            <div class="form-group">
                                <div class="col-sm-4 text-right">
                                    <label class="control-label tooltip-float">&nbsp;</label>
                                </div>
                                <div class="col-sm-6" data-bind="with: $root.selectedAddress">
                                    <div>
                                        <label id="address1" data-bind="text: address1" class="form-control" style="border:none"></label>
                                    </div>
                                </div>
                            </div>
                            <!--/ko-->
                            <!-- ko if: $root.selectedAddress()!==undefined && $root.selectedAddress().telephone1!=='' && $root.selectedAddress().telephone1!== undefined-->
                            <div class="form-group" style="margin-top: -20px;">
                                <div class="col-sm-4 text-right">
                                    <label class="control-label tooltip-float">&nbsp;</label>
                                </div>
                                <div class="col-sm-6" data-bind="with: $root.selectedAddress">
                                    <div>
                                        <label id="telephone" data-bind="text: telephone1" class="form-control" style="border:none"></label>
                                    </div>
                                </div>
                            </div>
                            <!--/ko-->
                            <!-- ko if: $root.selectedCompanyContact()!==undefined && $root.selectedCompanyContact().email !=='' && $root.selectedCompanyContact().email !== undefined -->
                            <div class="form-group" style="margin-top: -20px;">
                                <div class="col-sm-4 text-right">
                                    <label class="control-label tooltip-float">&nbsp;</label>
                                </div>
                                <div class="col-sm-6" data-bind="with: $root.selectedCompanyContact">
                                    <div>
                                        <label id="email" data-bind="text: email" class="form-control" style="border:none"></label>
                                    </div>
                                </div>
                            </div>
                            <!--/ko-->
                        </div>

                        <!--here it ends-->
                        <div class="form-group">
                            <div class="col-lg-4 text-right">
                                <div class="bs-example-tooltips tooltip-float-width">
                                    <a class="input-group-addon" tabindex="-1" style="text-decoration: none; border: 0; cursor: pointer; border: 0" data-container="body" data-toggle="popover" data-placement="right" data-content="Enter Estimate Title" data-original-title="" title="">
                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                    </a>
                                </div>

                                <label class="control-label tooltip-float">Order Ref. No</label>
                            </div>
                            <div class="col-lg-6">
                                <div>
                                    <input type="text" value="Double-Sided A4" placeholder="Enter Estimate Title" class="form-control" data-bind="value:orderReff" />

                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-lg-4 text-right">
                                <label class="control-label text-right tooltip-float">Carrier Tracking Path</label>
                            </div>
                            <div class="col-lg-6">
                                <input type="text" class="form-control" id="datepickerDate">

                            </div>
                        </div>
                    </div>


                    <div class="col-lg-6">

                        <div class="form-group">
                            <div class="col-sm-4 text-right" style="padding-top:10px;">
                                <label class="control-label text-right tooltip-float">Delivery Status</label>
                            </div>
                            <div class="col-sm-6">
                                <input type="text" style="height: 60px;" data-bind="value: isStatus()==19 ? 'Awaiting' :'Posted'" readonly class="form-control font24px" id="code" />
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-4 text-right">
                                &nbsp;
                            </div>
                            <div class="col-sm-6">
                                <select class="form-control" data-bind="options: $root.sectionFlags, optionsText: 'name', optionsValue: 'id', optionsCaption: 'Select a flag',value:flagId"></select>
                                <span class="help-block text-right"></span>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-lg-4 text-right">

                                <label class="control-label text-right tooltip-float">Carriers</label>
                            </div>
                            <div class="col-lg-6">
                                <select class="form-control" data-bind="options: $root.systemUsers, optionsText: 'fullName', optionsValue: 'id', optionsCaption: 'Select a system user',value:supplierId"></select>
                                <span class="help-block text-right"></span>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-lg-4 text-right">

                                <label class="control-label text-right tooltip-float">Carrier Phone</label>
                            </div>
                            <div class="col-lg-6">
                                <input type="text" class="form-control" data-bind="value:supplierTelNo">

                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-lg-4 text-right">

                                <label class="control-label text-right tooltip-float">Consignment Number</label>
                            </div>
                            <div class="col-lg-6">
                                <input type="text" class="form-control" id="datepickerDate" data-bind="vlaue:csNo">

                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-lg-4 text-right">

                                <label class="control-label text-right tooltip-float">Delivery Date</label>
                            </div>
                            <div class="col-lg-6">
                                <input type="text" style="height:60px;" class="form-control" data-bind="datepicker:deliveryDate">
                            </div>
                        </div>
                    </div>


                    @* <div class="col-lg-9 form-horizontal">*@

                    @*</div>*@
                </div>
                <div class="col-sm-12 form-horizontal">
                    <div class="col-sm-12">
                        <div class="form-group">
                            <div class="col-sm-2">
                                &nbsp;
                            </div>
                            <div class="col-sm-9">
                                <div id="orderHeaderProductsContainer" style=" background-color: gainsboro; border-radius: 3px;">
                                    <header class="main-box-header clearfix">
                                        <div id="orderProducts" style="float:left;"><span style="font-size:24px">Invoiced  Items</span></div>
                                    </header>
                                    <div class="main-box-body clearfix">
                                        <div class="table-responsive">
                                            <table class="table table-striped table-hover">
                                                <thead>
                                                    <tr>
                                                        <th class="col-sm-8"><span>Detail</span></th>

                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @*<!--ko if: nonDeliveryItems().length==0-->
                                                        <tr>
                                                            <td>
                                                                <span data-bind="text: 'No Items found!'"></span>
                                                            </td>
                                                            <td></td>
                                                            <td></td>
                                                        </tr>
                                                        <!-- /ko -->
                                                        <!-- ko template: { name: 'itemTemplate', foreach: nonDeliveryItems } -->
                                                        <!-- /ko -->*@
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                    <div class="btn-group" style="float: left; margin-top: 10px;">
                                        <button type="button" class="btn btn-primary">Add Product</button>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-12 form-horizontal main-box-body clearfix">
                    <div class="form-group">
                        <div class="col-lg-2 text-right">
                            <div class="bs-example-tooltips tooltip-float-width">
                                <a class="input-group-addon" tabindex="-1" style="text-decoration: none; border: 0; cursor: pointer; border: 0" data-container="body" data-toggle="popover" data-placement="right" data-content="Select estimate date." data-original-title="" title="">
                                    <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                </a>
                            </div>
                            <label class="control-label text-right tooltip-float">Comments</label>
                        </div>
                        <div class="col-lg-9" style="padding-right: 5px;padding-left: 5px;">
                            <textarea class="form-control" id="EstimateHeader" rows="3" data-bind="value: comments"></textarea>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-lg-2 text-right">
                            <div class="bs-example-tooltips tooltip-float-width">
                                <a class="input-group-addon" tabindex="-1" style="text-decoration: none; border: 0; cursor: pointer; border: 0" data-container="body" data-toggle="popover" data-placement="right" data-content="Select estimate date." data-original-title="" title="">
                                    <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                </a>
                            </div>
                            <label class="control-label text-right tooltip-float">Foot Notes</label>
                        </div>
                        <div class="col-lg-9" style="padding-right: 5px;padding-left: 5px;">
                            <textarea class="form-control" id="EstimateHeader" rows="3" data-bind="value: userNotes"></textarea>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-lg-2 text-right">
                            <div class="bs-example-tooltips tooltip-float-width">
                                <a class="input-group-addon" tabindex="-1" style="text-decoration: none; border: 0; cursor: pointer; border: 0" data-container="body" data-toggle="popover" data-placement="right" data-content="Select Source for Estimate." data-original-title="" title="">
                                    <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                </a>
                            </div>
                            <label class="control-label text-right tooltip-float">Raised By</label>
                        </div>
                        <div class="col-lg-3" style="padding-right: 5px;">
                            <select class="form-control">
                                <option>Lucas Eyre</option>
                                <option>Mark Temp</option>
                                <option>Smit Jones</option>
                                <option>Stephen</option>
                                <option>Gary McCutting</option>
                            </select>
                            <span class="help-block text-right"></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    function addAddress() {
        var e = document.getElementById("popoveraddress");
        var p = document.getElementById("addresspullicon");

        if (e.style.display == "none") {
            p.className = "fa fa-chevron-circle-down drop-icon";
            e.style.display = "block";
        }
        else if (e.style.display == "block") {
            p.className = "fa fa-chevron-circle-right drop-icon";
            e.style.display = "none";

        }
    }
</script>
