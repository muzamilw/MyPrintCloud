@{
    ViewBag.Title = "Delivery Notes";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section leftmenue {
}
<script src="~/Content/themes/Centaurus/js/jquery.js"></script>
<script>
    $(document).ready(function () {

        $(".edit-pertial").hide();
        $(".patial-edit").click(function () {

            $("#main-body").hide();
            $(".edit-pertial").show();


        });
        $("#closeBtn").click(function () {

            $("#main-body").show();
            $(".edit-pertial").hide();

        });
    });
</script>

<div class="row" id="orderBinding">
    <div class="row">
        <div class="col-lg-12">
            <div class="row">
                <div class="col-lg-12" id="topArea" style="padding-left:16px;">
                    <ol class="breadcrumb">
                        <li><a href="~/Dashboard/Index">Home</a></li>
                        <li class="active"><span>Delivery Notes</span></li>
                    </ol>
                    <div class="clearfix">
                        <h1 class="pull-left">Delivery Notes</h1>
                    </div>

                    <div class="col-lg-12" id="filter" style="height:50px;">
                        <div class="col-lg-10">
                            <div class="filter-block pull-left col-lg-7" style="margin-top:0px;">
                                <div class="col-lg-1 pull-left spanfilterlbl">
                                    &nbsp;
                                </div>
                                <div class="form-group pull-left">
                                    <input type="text" class="form-control" placeholder="Search...">
                                    <i class="fa fa-search search-icon"></i>
                                </div>
                            </div>
                            <div class="text-center col-lg-3" style="padding-left:0px !important;">
                                <label class="control-label">Filter By</label>
                                <div class="btn-group">
                                    <button type="button" class="btn btn-info"><i class="fa fa-flag"></i></button>
                                    <button type="button" class="btn btn-info dropdown-toggle" data-toggle="dropdown">
                                        <span class="caret"></span>
                                    </button>
                                    <ul class="dropdown-menu" role="menu">
                                        <li><span class="label label-warning"><i class="fa fa-flag"></i></span><label class="control-label">  High Priority</label></li>
                                        <li><span class="label label-danger"><i class="fa fa-flag"></i></span><label class="control-label">  Estimate Not Complte</label></li>
                                        <li><span class="label label-success"><i class="fa fa-flag"></i></span><label class="control-label">  Ready for Order</label></li>
                                        <li><span class="label label-default"><i class="fa fa-flag"></i></span><label class="control-label">  Normal</label></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-2">
                            <div class="pull-right">
                                <a class="btn btn-primary patial-edit">
                                    <i class="fa fa-plus-circle fa-lg"></i>&nbsp;Add Delivery Note
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

