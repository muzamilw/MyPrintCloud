<script src="~/Content/themes/Centaurus/js/jquery.js"></script>
<script>
    $(document).ready(function ()
    {

        $(".edit-pertial").hide();
        $(".partial-PO").hide();
        $("#Carriers-PO").hide();
        $("#DeliveryDate-PO").hide();
        $("#Reference1-PO").hide();
        $("#Reference2-PO").hide();

        $(".patial-edit").click(function ()
        {

            $("#main-body").hide();
            $(".edit-pertial").show();
          
          

        });
        $("#closeBtn").click(function ()
        {

            $("#main-body").show();
            $(".edit-pertial").hide();

        });
    });
</script>

