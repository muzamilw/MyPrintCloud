    </div>
    <div class="col-lg-12 form-horizontal">
        <h3 class="group-header">Header & Footer</h3>
        <div class="col-lg-6">

            <div class="form-group">
                <div class="col-lg-4 text-right">
                    <div class="bs-example-tooltips tooltip-float-width">

                        <a class="input-group-addon" data-container="body" data-toggle="popover" data-placement="right" data-content="Enter Order Header" data-original-title="" title="">
                            <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                        </a>

                    </div>



                    <label class="control-label tooltip-float">Order Header</label>

                </div>
                <div class="col-lg-6">
                    <div>
                        <textarea class="form-control" placeholder="Enter Order Header" id="EstimateHeader" rows="3" data-bind="value: headNotes"></textarea>


                    </div>

                </div>
            </div>


            <div class="form-group">
                <div class="col-lg-4 text-right">
                    <div class="bs-example-tooltips tooltip-float-width">

                        <a class="input-group-addon" data-container="body" data-toggle="popover" data-placement="right" data-content="Enter Order Footer" data-original-title="" title="">
                            <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                        </a>

                    </div>



                    <label class="control-label tooltip-float">Order Footer</label>

                </div>
                <div class="col-lg-6">
                    <div>
                        <textarea class="form-control" placeholder="Enter Order Footer" id="estimateFooter" rows="3" data-bind="value: footNotes"></textarea>


                    </div>

                </div>
            </div>
        </div>

        @* ReSharper restore UnknownCssClass *@

    </div>
</div>

