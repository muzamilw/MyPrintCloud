<div class="modal fade" id="sectionCostCenterDialogModel" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div style="width:838px;" class="modal-dialog">
        <div class="modal-content" data-bind="with: $root.selectedSectionCostCenter">
            <div class="modal-header" id="dialgHeaderPadding">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">Web Order Cost Center</h4>
            </div>
            <div class="col-lg-12 outerDiv-Padding form-horizontal">
                <div class="col-lg-6">
                    <div class="col-lg-4">
                        <label class="text-right"><b>Customer Name</b></label>
                        <label class="text-right"><b>Estimate Title</b></label>
                        <label class="text-right"><b>Item Name</b></label>
                        <label class="text-right"><b>Section Name</b></label>
                    </div>
                    <div class="col-lg-8">
                        <label data-bind="text: $root.selectedOrder().companyName()"></label><br />
                        <label data-bind="text: $root.selectedOrder().name()"></label><br />
                        <label data-bind="text: $root.selectedProduct().productName()"></label><br />
                        <label data-bind="text: $root.selectedSection().name()"></label>
                    </div>
                </div>
            </div>
            <div class="col-lg-12">
                <div class="col-lg-12 modal-header" id="dialgHeaderPadding">
                    <h4 class="modal-title">Work Instructions</h4>
                </div>
                <div class="col-lg-6 outerDiv-Padding form-horizontal">
                    <textarea data-bind="text: $root.selectedQty() == 1 ? qty1WorkInstructions() :$root.selectedQty() == 2? qty2WorkInstructions() : qty3WorkInstructions()"></textarea>
                </div>
            </div>
            <div class="col-lg-12">

                <div class="col-lg-6"></div>
                <div class="col-lg-6">
                    <div class="form-group ">
                        <input type="checkbox" data-bind="checked: isDirectCost" />
                        <label> Direct Cost</label>
                    </div>
                    <div class="form-group ">
                        <input type="checkbox" data-bind="checked: isPurchaseOrderRaised" />
                        <label> Raise Purchase Order</label>
                    </div>

                    @*<div class="col-lg-6">*@
                    <div class="form-group col-lg-12">
                        <div class="col-lg-4 text-right"><label class="text-right">Quantity</label></div>
                        <div class="col-lg-8">
                            <!--ko if:  $root.selectedQty() == 1-->
                            <input type="text" class="text-right" data-bind="value: qty1 " />
                            <!--/ko-->
                            <!--ko if:  $root.selectedQty() == 2-->
                            <input type="text" class="text-right" data-bind="value: qty2 " />
                            <!--/ko-->
                            <!--ko if:  $root.selectedQty() == 3-->
                            <input type="text" class="text-right" data-bind="value: qty3 " />
                            <!--/ko-->
                            <input type="button" value="Reset" style="float: right;" />
                        </div>
                    </div>
                    <div class="form-group col-lg-12">
                        <div class="col-lg-4 text-right"><label class="text-right">Total Cost</label></div>
                        <div class="col-lg-8">
                            <!--ko if:  $root.selectedQty() == 1-->
                            <input type="text" class="text-right" data-bind="value: qty1EstimatedStockCost " />
                            <!--/ko-->
                            <!--ko if:  $root.selectedQty() == 2-->
                            <input type="text" class="text-right" data-bind="value: qty2EstimatedStockCost " />
                            <!--/ko-->
                            <!--ko if:  $root.selectedQty() == 3-->
                            <input type="text" class="text-right" data-bind="value: qty3EstimatedStockCost " />
                            <!--/ko-->

                        </div>
                    </div>
                    <div class="form-group col-lg-12">
                        <div class="col-lg-4 text-right"><label class="text-right">Total Price</label></div>
                        <div class="col-lg-8">
                            <!--ko if:  $root.selectedQty() == 1-->
                            <input type="text" class="text-right" data-bind="value: qty1Charge " />
                            <!--/ko-->
                            <!--ko if:  $root.selectedQty() == 2-->
                            <input type="text" class="text-right" data-bind="value: qty2Charge " />
                            <!--/ko-->
                            <!--ko if:  $root.selectedQty() == 3-->
                            <input type="text" class="text-right" data-bind="value: qty3Charge " />
                            <!--/ko-->
                        </div>
                    </div>
                    <div class="form-group col-lg-12">
                        <div class="col-lg-4 text-right"><label class="text-right">Markup</label></div>
                        <div class="col-lg-6"><select class="form-control col-lg-4" data-bind="options:$root.markups,optionsText:'MarkUpName',optionsValue:'MarkUpId', value:$root.selectedQty() == 1 ? qty1MarkUpId :$root.selectedQty() == 2? qty2MarkUpId : qty3MarkUpId  "></select></div>
                    </div>
                    <div class="form-group col-lg-12">
                        <div class="col-lg-4 text-right"><label class="text-right">Markup Value</label></div>
                        <div class="col-lg-8">
                            <!--ko if:  $root.selectedQty() == 1-->
                            <input type="text" class="text-right" data-bind="value: qty1MarkUpValue " readonly="readonly" />
                            <!--/ko-->
                            <!--ko if:  $root.selectedQty() == 2-->
                            <input type="text" class="text-right" data-bind="value: qty2MarkUpValue " readonly="readonly" />
                            <!--/ko-->
                            <!--ko if:  $root.selectedQty() == 3-->
                            <input type="text" class="text-right" data-bind="value: qty3MarkUpValue " readonly="readonly" />
                            <!--/ko-->

                        </div>
                    </div>
                    <div class="form-group col-lg-12">
                        <div class="col-lg-4 text-right"><label class="text-right">Net Total</label></div>
                        <div class="col-lg-8">
                            <!--ko if:  $root.selectedQty() == 1-->
                            <input type="text" class="text-right" data-bind="value: qty1NetTotal " />
                            <!--/ko-->
                            <!--ko if:  $root.selectedQty() == 2-->
                            <input type="text" class="text-right" data-bind="value: qty2NetTotal " />
                            <!--/ko-->
                            <!--ko if:  $root.selectedQty() == 3-->
                            <input type="text" class="text-right" data-bind="value: qty3NetTotal " />
                            <!--/ko-->

                        </div>
                    </div>

                    @*</div>*@
                    @*<div class="col-lg-6">



                            <input type="text"/>
                            <input type="text"/>

                        </div>*@


                </div>
            </div>

            <div class="modal-footer">
                <hr />
            </div>
        </div>
    </div>
</div>
