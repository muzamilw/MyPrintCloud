<div class="modal fade" id="productFromRetailStoreModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" data-backdrop="static" aria-hidden="true">
    <div class="modal-dialog" id="productFromRetailStoreModalDialog">
        <div class="modal-content">
            <div class="modal-header" id="dialgHeaderPadding">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">Product From Retail Store</h4>
            </div>

            <div class="col-lg-12 col-lg-12 outerDiv-Padding form-horizontal">
                <div class="col-lg-6 center_align form-horizontal outerDiv-Padding" id="OrderDialogSection1">
                    <header class="main-box-header clearfix">
                        <div class="filter-block pull-left">
                            <div class="form-group pull-left">
                                <input type="text" class="form-control" placeholder="Search...">    @*data-bind="value: $root.searchCompanyTerritoryFilter, validationOnElement: $root.searchCompanyTerritoryFilter"*@
                                <i class="fa fa-search search-icon"></i>@*data-bind="click:$root.searchCompanyTerritory"*@
                            </div>
                        </div>
                    </header>
                    <div class="main-box-body clearfix">
                        <div class="table-responsive">
                            <table class="table table-striped table-hover">
                                <thead>
                                    <tr>
                                        <th class="col-lg-5">Name</th>
                                        <th class="col-lg-6">Code</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <!-- ko template: { name: 'itemsTemplate' , foreach: $root.orderProductItems } -->
                                    <!-- /ko -->
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="col-lg-5 center_align form-horizontal outerDiv-Padding" id="OrderDialogSection2">
                    <div class="form-group">
                        <div class="col-lg-4 text-right">
                            <div class="bs-example-tooltips tooltip-float-width">
                                <a class="input-group-addon" tabindex="-1" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Select Store" data-original-title="" title="">
                                    <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                </a>
                            </div>
                            <label for="txtSortOrder" class="control-label text-right tooltip-float">Select Stock</label>
                        </div>
                        <!--ko if: $root.selecteditem()!=undefined -->
                        <div class="col-lg-6">
                            <select class="form-control col-lg-4" data-bind="options:$root.selecteditem().itemStockOptions,optionsText:'label',optionsValue:'id', value: $root.selectedStockItem"></select>
                        </div>
                        <div class="main-box-body clearfix">
                            <div class="table-responsive">
                                <table class="table table-striped table-hover">
                                    <thead>
                                        <tr>
                                            <!-- ko if: $root.selecteditem().isQtyRanged() == 2 -->
                                            <th class="col-lg-5">Quantity</th>
                                            <!--/ko-->
                                            <!-- ko if: $root.selecteditem().isQtyRanged() == 1 -->
                                            <th class="col-lg-2">Quantity From</th>
                                            <th class="col-lg-2">Quantity To</th>
                                            <!--/ko-->
                                            <th class="col-lg-6" data-bind="text: $root.selectedStockOptionName"></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <!-- ko template: { name: 'itemPriceMatrixTemplate' , foreach: $root.selecteditem().itemPriceMatrices } -->
                                        <!-- /ko -->
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div id="AddOnHeading " class="col-lg-4 text-right">Add Ons</div>
                        <div class="main-box-body clearfix">
                            <div class="table-responsive">
                                <table class="table table-striped table-hover">
                                    <thead>
                                        <tr>
                                            <th class="col-lg-2"></th>
                                            <th class="col-lg-5">Cost Center</th>
                                            <th class="col-lg-5">Total</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <!-- ko template: { name: 'addOnTemplate' , foreach: $root.selectedStockOption().itemAddonCostCentres } -->
                                        <!-- /ko -->
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <!--/ko-->
                    </div>
                </div>
            </div>

            <div class="modal-footer">
                <hr />
                <button type="button" class="btn btn-primary">Add</button>    @*data-bind="click: $root.onSaveCompanyTerritory"*@
                @*<button type="button" class="btn btn-default" data-bind="click: $root.onCloseProductFromRetailStore" style="margin-left: 0px;">Close</button>*@
            </div>
        </div>
    </div>
</div>
<script id="itemsTemplate" type="text/html">
    <tr data-bind="click : $root.updateItemsDataOnItemSelection">
        <td data-bind="text: productName"></td>
        <td data-bind="text: productCode"></td>
    </tr>
</script>
<script id="addOnTemplate" type="text/html">
    <tr>
        <td> <input type="checkbox"/></td>
        <td data-bind="text: costCentreName"></td>
        <td data-bind="text: totalPrice"></td>
    </tr>
</script>
<script id="itemPriceMatrixTemplate" type="text/html">
    <tr>
        @*data-bind="click : $root.updateItemsDataOnItemSelection"*@
        <!-- ko if: $root.selecteditem().isQtyRanged() == 2 -->
        <td data-bind="text: quantity"></td>
        <!--/ko-->
        <!-- ko if: $root.selecteditem().isQtyRanged() == 1 -->
        <td data-bind="text: qtyRangedFrom"></td>
        <td data-bind="text: qtyRangedTo"></td>
        <!--/ko-->
        <!-- ko if: $root.selectedStockOptionSequenceNumber() == 1 -->
        <td data-bind="text: pricePaperType1"></td>
        <!--/ko-->
        <!-- ko if: $root.selectedStockOptionSequenceNumber() == 2 -->
        <td data-bind="text: pricePaperType2"></td>
        <!--/ko-->
        <!-- ko if: $root.selectedStockOptionSequenceNumber() == 3 -->
        <td data-bind="text: pricePaperType3"></td>
        <!--/ko-->
        <!-- ko if: $root.selectedStockOptionSequenceNumber() == 4 -->
        <td data-bind="text: pricePaperType4"></td>
        <!--/ko-->
        <!-- ko if: $root.selectedStockOptionSequenceNumber() == 5 -->
        <td data-bind="text: pricePaperType5"></td>
        <!--/ko-->
        <!-- ko if: $root.selectedStockOptionSequenceNumber() == 6 -->
        <td data-bind="text: pricePaperType6"></td>
        <!--/ko-->
        <!-- ko if: $root.selectedStockOptionSequenceNumber() == 7 -->
        <td data-bind="text: pricePaperType7"></td>
        <!--/ko-->
        <!-- ko if: $root.selectedStockOptionSequenceNumber() == 8 -->
        <td data-bind="text: pricePaperType8"></td>
        <!--/ko-->
        <!-- ko if: $root.selectedStockOptionSequenceNumber() == 9 -->
        <td data-bind="text: pricePaperType9"></td>
        <!--/ko-->
        <!-- ko if: $root.selectedStockOptionSequenceNumber() == 10 -->
        <td data-bind="text: pricePaperType10"></td>
        <!--/ko-->
        <!-- ko if: $root.selectedStockOptionSequenceNumber() == 11 -->
        <td data-bind="text: pricePaperType11"></td>
        <!--/ko-->
        @*<td data-bind="text: productCode"></td>*@
    </tr>
</script>
