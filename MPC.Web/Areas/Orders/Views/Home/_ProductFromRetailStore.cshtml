<div class="modal fade" id="productFromRetailStoreModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" data-backdrop="static" aria-hidden="true">
    <div class="modal-dialog" id="productFromRetailStoreModalDialog">
        <div class="modal-content">
            <div class="modal-header" id="dialgHeaderPadding">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">Product From Retail Store</h4>
            </div>
            
                <div class="col-lg-12 col-lg-12 outerDiv-Padding form-horizontal">
                    <div class="col-lg-6 center_align form-horizontal outerDiv-Padding" id="OrderDialogSection1">
                        <header class="main-box-header clearfix">
                            <div class="filter-block pull-left">
                                <div class="form-group pull-left">
                                    <input type="text" class="form-control" placeholder="Search...">    @*data-bind="value: $root.searchCompanyTerritoryFilter, validationOnElement: $root.searchCompanyTerritoryFilter"*@
                                    <i class="fa fa-search search-icon"></i>@*data-bind="click:$root.searchCompanyTerritory"*@
                                </div>
                            </div>
                        </header>
                        <div class="main-box-body clearfix">

                            <div class="table-responsive">
                                <table class="table table-striped table-hover">
                                    <thead>
                                        <tr>
                                            <th class="col-lg-5">Name</th>
                                            <th class="col-lg-6">Code</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <!-- ko template: { name: 'itemsTemplate' , foreach: $root.orderProductItems } -->
                                        <!-- /ko -->
                                    </tbody>
                                </table>
                            </div>
                        </div>

                    </div>
                <div class="col-lg-5 center_align form-horizontal outerDiv-Padding" id="OrderDialogSection2">
                    <div class="form-group">
                        <div class="col-lg-4 text-right">
                            <div class="bs-example-tooltips tooltip-float-width">
                                <a class="input-group-addon" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Select Store" data-original-title="" title="">
                                    <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                </a>
                            </div>

                            <label for="txtSortOrder" class="control-label text-right tooltip-float">Select Store</label>
                        </div>
                        <div class="col-lg-6">
                            <select class="form-control" @*data-bind="options: $root.companyBannerSetList,optionsCaption:'All Set', optionsText: 'setName', optionsValue: 'id',value:$root.filteredCompanySetId"*@></select>
                        </div>
                    </div>
                </div>
            </div>
          
                <div class="modal-footer">
                    <hr />
                    <button type="button" class="btn btn-primary">Save</button>    @*data-bind="click: $root.onSaveCompanyTerritory"*@
                    <button type="button" class="btn btn-default" data-bind="click: $root.onCloseProductFromRetailStore" style="margin-left: 0px;">Close</button>
                </div>
            </div>
    </div>
</div>

<script id="itemsTemplate" type="text/html">
    <tr data-bind="click : $root.updateItemsDataOnItemSelection">        @*data-bind="click: $root.selectCompanyTerritory, css: { selectedRow: $data === $root.selectedCompanyTerritory}"*@
        <td data-bind="text: productName"></td>
        <td data-bind="text: productCode"></td>
    </tr>
</script>
@* Wizard Popup ends here*@
