
<div class="modal fade my-modal" id="orderStatusProgressToJobModal" tabindex="-1" role="dialog" data-backdrop="static" aria-labelledby="myModalLabel" aria-hidden="true">
    <div style="width: 838px;" class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Progress to Job</h4>
            </div>
            <div class="modal-body form-horizontal clearfix" data-bind="with:$root.selectedItemForProgressToJobWizard">
                <div class=" form-horizontal clearfix">
                    <div class="col-lg-12 clearfix">
                        <div class="form-group">
                            <div class="col-lg-3 text-right">
                                <div class="bs-example-tooltips tooltip-float-width">
                                    <a class="input-group-addon" tabindex="-1" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Item Name" data-original-title="" title="">
                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                    </a>
                                </div>
                                <label for="name" class="control-label text-right tooltip-float">Item Name</label>
                            </div>
                            <div class="col-lg-5">
                                <label style="border: none" class="form-control" id="name" data-bind="text: productName"></label>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-lg-3 text-right">
                                <div class="bs-example-tooltips tooltip-float-width">

                                    <a class="input-group-addon" tabindex="-1" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Select Job Manager" data-original-title="" title="">
                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                    </a>

                                </div>
                                <label class="control-label text-right tooltip-float">Select Job Manager</label>
                            </div>
                            <div class="col-lg-5">
                                @*<select class="form-control" data-bind="options: $root.systemUsers, optionsText: 'fullName', optionsCaption: 'Select job manager', optionsValue: jobManagerUser "></select>*@ @*,value:jobManagerUser*@
                                <select class="form-control" id="systemUsers" data-bind="options: $root.systemUsers, optionsText: 'fullName', optionsValue: 'id',value:jobManagerUser"></select>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-lg-3 text-right">
                                <div class="bs-example-tooltips tooltip-float-width">
                                    <a class="input-group-addon" tabindex="-1" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Job starting date" data-original-title="" title="">
                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                    </a>
                                </div>
                                <label for="start" class="control-label text-right tooltip-float">Job Starting Date</label>
                            </div>
                            <div class="col-lg-5">
                                <input type="text" class="form-control" id="start" data-bind="datepicker:jobEstimatedStartDateTime">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-lg-3 text-right">
                                <div class="bs-example-tooltips tooltip-float-width">
                                    <a class="input-group-addon" tabindex="-1" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Job completion date" data-original-title="" title="">
                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                    </a>
                                </div>
                                <label for="end" class="control-label text-right tooltip-float">Job Completion Date</label>
                            </div>
                            <div class="col-lg-5">
                                <input type="text" class="form-control" id="end" data-bind="datepicker:jobEstimatedCompletionDateTime">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-bind="click:$root.clickOnJobToProgressWizard">Ok</button>
                @*<button type="button" class="btn btn-default" data-bind="click:$root.clickOnJobToProgressWizard">Cancel</button>*@
            </div>
        </div>
    </div>
</div>

@* Wizard Popup ends here*@
