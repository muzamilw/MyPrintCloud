
<div class="modal fade" id="productVideoDialog" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div style="width: 838px;" class="modal-dialog" data-bind="with: $root.activeVideo">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">Add Product Video</h4>
            </div>
            <div class="col-lg-12 form-horizontal outerDiv-Padding">
                <div class="form-group">
                    <div class="col-lg-3 text-right">
                        <div class="bs-example-tooltips tooltip-float-width">

                            <a class="input-group-addon" style="text-decoration: none; border: 0; cursor: pointer;" border 0" data-container="body" data-toggle="popover" data-placement="right" data-content="Enter Meta Title" data-original-title="" title="">
                                <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                            </a>

                        </div>


                        <label for="txtMetaTitle" class="control-label text-right tooltip-float">YouTube Video Link</label>

                    </div>

                    <div class="col-lg-6">
                        <input type="text" class="form-control" id="txtMetaTitle" placeholder="Enter Youtube Link" data-bind="value: videoLink">


                    </div>

                </div>
                <div class="form-group">
                    <div class="col-lg-3 text-right">
                        <div class="bs-example-tooltips tooltip-float-width">

                            <a class="input-group-addon" style="text-decoration: none; border: 0; cursor: pointer;" border 0" data-container="body" data-toggle="popover" data-placement="right" data-content="Enter Meta Title" data-original-title="" title="">
                                <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                            </a>

                        </div>


                        <label for="txtMetaTitle" class="control-label text-right tooltip-float">Caption</label>

                    </div>

                    <div class="col-lg-6">
                        <input type="text" class="form-control" id="txtMetaTitle" placeholder="Enter Caption" data-bind="value: caption">
                        <span class="help-block text-right"></span>
                    </div>

                </div>

            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-bind="click: $root.closeVideoDialog">Close</button>
                <button type="button" class="btn btn-primary" data-bind="click: $root.saveVideo">Save</button>
            </div>
        </div>
    </div>
</div>


@* Wizard Popup ends here*@