
<div class="modal fade my-modal" id="myEditBannerModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div style="width: 838px;" class="modal-dialog" data-bind="with:$root.selectedCompanyBanner">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">Banner Detail</h4>
            </div>
            <div class="col-lg-12 form-horizontal outerDiv-Padding">
                <div class="form-group">
                    <div class="col-lg-5 text-right">
                        <div class="bs-example-tooltips tooltip-float-width">

                            <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Product Page Banner" data-original-title="" title="">
                                <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                            </a>

                        </div>
                        <label class="control-label text-right tooltip-float">Banner main Image (PNG 400 X 250 pixels)</label>
                    </div>
                    <div class="col-lg-4">
                        @*<img style="min-height: 200px; width: 270px;" data-bind="attr: { src: fileBinary }" />*@
                        <img height="200" width="270" data-bind="visible:imageSource()===undefined,attr: { src: '@Url.Content("~/")' + filePathWithCacheRemoveTechnique() }" />
                        <img height="200" width="270" data-bind="visible:imageSource()!==undefined,attr: { src: imageSource }" />

                    </div>
                    <div class="col-lg-1">
                        <div class="fileUpload btn btn-primary" data-bind="click:$root.openMediaLibraryDialogFromCompanyBanner">
                            <span>....</span>
                            @*<input type="file" class="upload" id="image_files" data-bind="files: { onLoaded: $root.MultipleImageFilesLoadedCallback, fileFilter: 'image.*'}" />*@
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-lg-5 text-right">
                        <div class="bs-example-tooltips tooltip-float-width">

                            <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Select Product Type" data-original-title="" title="">
                                <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                            </a>

                        </div>
                        <label class="control-label text-right tooltip-float">Banner set</label>
                    </div>
                    <div class="col-lg-5">
                        <select class="form-control" data-bind="options: $root.companyBannerSetList, optionsText: 'setName', optionsValue: 'id',value:companySetId, validationTooltip: companySetId"></select>
                    </div>
                    <div class="col-lg-2">
                        <div class="btn btn-primary pull-left" data-bind="click:$root.onAddSetBanner">
                            ...
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-lg-5 text-right">

                        <div class="bs-example-tooltips tooltip-float-width">

                            <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Sort Order in Category" data-original-title="" title="">
                                <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                            </a>

                        </div>


                        <label for="txtBannerHead" class="control-label text-right tooltip-float">Banner Heading</label>

                    </div>

                    <div class="col-lg-5">
                        <input type="text" class="form-control" id="txtBannerHead" data-bind="value:heading, validationTooltip:heading">
                    </div>

                </div>
                <div class="form-group">
                    <div class="col-lg-5 text-right">

                        <div class="bs-example-tooltips tooltip-float-width">

                            <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Sort Order in Category" data-original-title="" title="">
                                <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                            </a>

                        </div>


                        <label for="txtSortOrder" class="control-label text-right tooltip-float">Description</label>

                    </div>

                    <div class="col-lg-5">
                        <input type="text" class="form-control" id="txtdesc" data-bind="value:description">
                    </div>

                </div>
                <div class="form-group">
                    <div class="col-lg-5 text-right">

                        <div class="bs-example-tooltips tooltip-float-width">

                            <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Sort Order in Category" data-original-title="" title="">
                                <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                            </a>

                        </div>


                        <label for="txtSortOrder" class="control-label text-right tooltip-float">Item URL</label>

                    </div>

                    <div class="col-lg-5">
                        <input type="text" class="form-control" id="txtItemUrl" data-bind="value:itemURL">
                    </div>

                </div>
                <div class="form-group">
                    <div class="col-lg-5 text-right">

                        <div class="bs-example-tooltips tooltip-float-width">

                            <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Sort Order in Category" data-original-title="" title="">
                                <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                            </a>

                        </div>


                        <label for="txtSortOrder" class="control-label text-right tooltip-float">Button URL</label>

                    </div>

                    <div class="col-lg-5">
                        <input type="text" class="form-control" id="txtBtnUrl" data-bind="value:buttonURL">
                    </div>

                </div>
            </div>
            <div class="modal-footer">
                <hr />
                <button type="button" class="btn btn-primary" data-bind="click:$root.onSaveCompanyBanner, enable: hasChanges, css: { 'btn-warning': hasChanges }">Save</button>
                <button type="button" class="btn btn-default" data-bind="click:$root.onCloseCompanyBanner" style="margin-left: 0px;">Close</button>
                <button type="button" class="btn btn-danger archiveButton" data-bind="click:$root.onDeleteCompanyBanner, enable: $root.selectedCompanyBanner().id() != undefined">Archive</button>
            </div>
        </div>
    </div>
</div>


@* Wizard Popup ends here*@
