                                                <ol class="dd-list">
                                                    <!-- ko template: { name: 'invoiceAndShippedItemTemplate', foreach: $root.items }-->
                                                    <!-- /ko -->
                                                </ol>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>

    </div>

</div>
