                                                    <!-- /ko -->
                                                </ol>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-2">
                            <div class="form-group collapsableView">
                                <div class="panel-group" id="accordion6">
                                    <div class="panel panel-default">
