@{
    ViewBag.Title = "Company Detail";
}
@section leftmenue {
}
<div class="row" data-bind="visible: $root.isEditorVisible" style="display: none">
    <div class="col-lg-12">
        <div class="row">
            <div class="col-lg-12">
                <ol class="breadcrumb">
                    <li><a href="~/Dashboard/Index">Home</a></li>
                    <li><a href="/CRM/SuppliersList">CRM</a></li>
                    <li class="active"><span>Customers</span></li>
                </ol>
                <!--ko if: $root.selectedStore() != undefined -->
                <h1 data-bind="text: $root.selectedStore().name()"></h1>
                <!-- /ko -->
                
            
            </div>
        </div>
        <div class="row" id="user-profile">
            <div class="col-lg-12 col-md-8 col-sm-8" style="padding-bottom: 10px;">
                <div class="clearfix ">
                    <div class="col-xs-4 pull-left validationSummaryContainer">
                        <a class="validationSummaryLink" data-bind="visible: $root.errorList().length > 0, bootstrapPopover: { html: true, title: 'Please fix following errors', elementNode: 'validationSummaryPopupForStore', popoverId: 'validationSummaryPopoverBodyForStore' }" style="display: none">Validation Errors</a>
                        <div class="well well-small validationSummaryModal" id="validationSummaryPopupForStore" style="display: none">
                            <div id="validationSummaryPopoverBodyForStore">
                                <ul data-bind="foreach: $root.errorList">
                                    <li>
                                        <a data-bind="click: $root.gotoElement.bind($data.element)"><span data-bind="text: name"></span></a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-4">
                    </div>
                    <div class="col-xs-4">
                    </div>
                    <div class="col-xs-12" style="text-align: right; padding-right: 0px;" data-bind="with: $root.selectedStore()">
                        <button class="btn  btn-info" id="saveBtn" data-bind="click:$root.saveStore, enable: hasChanges, css: { 'btn-warning': hasChanges }">Save</button>
                        <button class="btn btn-default" id="saveBtn" data-bind="click: $root.closeEditDialog">Close</button>
                        @*<button type="button" class="btn btn-primary btn-lg" data-bind="click: $root.saveStore">
                                <span class="fa fa-save"></span>&nbsp;Save
                            </button>
                            <button type="button" class="btn btn-primary btn-lg" data-bind="click: $root.closeEditDialog">
                                <span class="fa fa-save"></span>&nbsp;Close
                            </button>*@
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="main-box clearfix">
                    <div class="tabs-wrapper profile-tabs">
                        <ul class="nav nav-tabs">
                            <li class="active" id="generalInfoTabForCrm"><a href="#tab-newsfeedForCrm" data-toggle="tab">General Info</a></li>
                            <li><a href="#tab-territoryForCrm" data-toggle="tab" data-bind="click: $root.userAndAddressesTabSelected">User & Addresses</a></li>
                            <!--ko if:$root.selectedStore() != undefined && $root.selectedStore().companyId() != undefined-->
                            <!--ko if: $root.isProspectOrCustomerScreen() -->
                            <li><a href="#tab-DAM" data-toggle="tab">DAM</a></li>
                            <!-- /ko -->
                            <li><a href="#tab-orders" data-toggle="tab" data-bind="click: $root.ordersTabClickHandler">Orders</a></li>
                            <li><a href="#tab-invoices" data-toggle="tab" data-bind="click: $root.invoicesTabClickHandler">Invoices</a></li>
                            <!-- /ko -->
                            @*<li><a href="#tab-contacts" data-toggle="tab">Users</a></li>*@
                            @* <li><a href="#tab-WebDetails" data-toggle="tab">Web Details</a></li>*@
                        </ul>
                        <div class="tab-content" data-bind="with: $root.selectedStore">
                            <div class="tab-pane fade in active" id="tab-newsfeedForCrm">
                                <div class="col-lg-12 form-horizontal">
                                    <h3 class="group-header">Store Details</h3>
                                    <div class="col-lg-9">
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">
                                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Name of the store that will be displayed on the web." data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>
                                                </div>
                                                <label for="txtCompanyName" class="control-label text-right tooltip-float">Store Name</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" name="Store Name" class="form-control" id="txtCompanyName" placeholder="Enter Store Name" data-bind="value: name, validationTooltip: name, validationOnElement: name">
                                                <span class="help-block text-right">This will be used in defalt URL to pass on to your customer.</span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">
                                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Code to access your store http://pinkcards.com/corporatedemo/login" data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>
                                                </div>
                                                <label for="txtWebAccessCode" class="control-label text-right tooltip-float">Web Access Code</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" name="Web Access Code" class="form-control" id="txtWebAccessCode" placeholder="Enter Web Access Code" data-bind="value: webAccessCode, validationTooltip: webAccessCode, validationOnElement: webAccessCode">
                                                <span class="help-block text-right">URL http://pinkcards.com/corporatedemo/login</span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">
                                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="ex. (999) 999-9999" data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>
                                                </div>
                                                <label class="control-label tooltip-float" for="maskedPhone">Phone</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <div class="input-group">
                                                    <span class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus"><i class="fa fa-phone"></i></span>
                                                    <input type="text" class="form-control" id="maskedPhone" data-bind="value: phoneNo">
                                                </div>
                                                <span class="help-block">ex. (999) 999-9999</span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">
                                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="User notes for the store." data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>
                                                </div>
                                                <label for="txtNotes" class="control-label text-right tooltip-float">Notes</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <textarea class="form-control" id="txtNotes" rows="5" data-bind="value: notes"></textarea>
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-3 col-md-4 col-sm-4">
                                        <div class="main-box clearfix">
                                            <header class="main-box-header clearfix textAlignCenter">
                                                <h2><label data-bind="text: name"></label></h2>
                                            </header>
                                            <div class="main-box-body clearfix">
                                                <div class="profile-status">
                                                </div>
                                                <img style="min-height: 150px; width: 150px;" data-bind="attr: { src: storeImageFileBinary }" />
                                                <div class="pull-right">
                                                    <div class="fileUpload btn btn-primary">
                                                        <span>....</span>
                                                        <input type="file" class="upload" id="image_files" multiple data-bind="files: { onLoaded: $root.storeImageFilesLoadedCallback, fileFilter: 'image.*'}" />
                                                    </div>
                                                </div>
                                                <div class="profile-label profileLabelPadding">
                                                    <!-- ko if: status() == 1 -->
                                                    <span class="label label-success">Active</span>
                                                    <!-- /ko -->
                                                    <!-- ko if: status() != 1 -->
                                                    <span class="label label-danger">DeActive</span>
                                                    <!-- /ko -->
                                                </div>
                                                <div class="profile-stars">
                                                    <span data-bind="value: type"></span>
                                                </div>
                                                <div class="profile-since" data-bind="value: accountOpenDate">
                                                </div>
                                                <div class="profile-details">
                                                    <ul class="fa-ul">
                                                        <li><i class="fa-li fa fa-truck"></i>New orders in last <span>5</span> days</li>
                                                        <li><i class="fa-li fa fa-comment"></i>New users in last <span>5</span> days</li>
                                                        <li><i class="fa-li fa fa-tasks"></i>Tasks/Events: <span>1024</span></li>
                                                    </ul>
                                                </div>
                                                <div class="profile-message-btn center-block text-center">
                                                    <a href="#" class="btn btn-success">
                                                        <i class="fa fa-envelope"></i>
                                                        Send message
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-12 form-horizontal">
                                    <h3 class="group-header">Account Details</h3>
                                    <div class="col-lg-9">
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">
                                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Opening date of account - format mm-dd-yyyy" data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>
                                                </div>
                                                <label for="datepickerDate" class="control-label text-right tooltip-float">Account Opened</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" class="form-control" id="datepickerDate" data-bind="datetimepicker: accountOpenDate, format: 'DD MMM YYYY'">
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">
                                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Store's Account Manager " data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>
                                                </div>
                                                <label class="control-label text-right tooltip-float">Account Manager</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <select class="form-control col-lg-4" data-bind="options:$root.systemUsers,optionsText:'userName',optionsValue:'systemUserId', value: $root.selectedStore().accountManagerId"></select>   @*,value: $root.selectedClaimFilterListitem*@
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">
                                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="GST registration number will be used for invoices" data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>
                                                </div>
                                                <label for="txtGSTRegNo" class="control-label text-right tooltip-float">GST Reg. No.</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" class="form-control" id="txtGSTRegNo" placeholder="Enter GST Registration Number" data-bind="value: avatRegNumber">
                                                <span class="help-block text-right">GST registration number</span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">
                                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="GST reference number will be used for accounts" data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>
                                                </div>
                                                <label for="txtGSTRefNo" class="control-label text-right  tooltip-float">GST Ref. No.</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" class="form-control" id="txtGSTRefNo" placeholder="Enter GST Reference Number" data-bind="value: avatRegReference">
                                                <span class="help-block text-right">GST reference number</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-12 form-horizontal">
                                    <h3 class="group-header">Social Details</h3>
                                    <div class="col-lg-9">
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">
                                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Your Facebook Page URL - e.g. https://www.facebook.com/Pinkcards" data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>
                                                </div>
                                                <label for="txtFacebookURL" class="control-label text-right tooltip-float">Facebook URL</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" class="form-control" id="txtFacebookURL" placeholder="Enter Facebook URL" data-bind="value: facebookUrl">
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">
                                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Facilitate user to login/Register from their Facebook account. You can get Your Facebook app Id from https://developers.facebook.com/apps" data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>
                                                </div>
                                                <label for="txtFacebookAppID" class="control-label text-right tooltip-float">Facebook APP ID</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" class="form-control" id="txtFacebookAppID" placeholder="Enter Facebook App ID" data-bind="value: facebookAppId">
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">
                                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Facilitate user to login/Register from their Facebook account. You can get Your Facebook app key from https://developers.facebook.com/apps" data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>
                                                </div>
                                                <label for="txtFacebookAppKey" class="control-label text-right tooltip-float">Facebook APP Key</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" class="form-control" id="txtFacebookAppKey" placeholder="Enter Facebook App Key" data-bind="value: facebookAppKey">
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">
                                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Your Twitter Page URL - e.g. https://www.twitter.com/Pinkcards " data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>
                                                </div>
                                                <label for="txtTwitterURL" class="control-label text-right tooltip-float">Twitter URL</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" class="form-control" id="txtTwitterURL" placeholder="Enter Twitter URL" data-bind="value: twitterUrl">
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">
                                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Facilitate user to login/Register from their twitter account. You can get Your Twitter app Id from https://dev.twitter.com/" data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>
                                                </div>
                                                <label for="txtTwitterAppID" class="control-label text-right tooltip-float">Twitter APP ID</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" class="form-control" id="txtTwitterAppID" placeholder="Enter Twitter App ID" data-bind="value: twitterAppId">
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">
                                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Facilitate user to login/Register from their twitter account. You can get Your Twitter app Id from https://dev.twitter.com/" data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>
                                                </div>
                                                <label for="txtTwitterAppKey" class="control-label text-right tooltip-float">Twitter APP Key</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" class="form-control" id="txtTwitterAppKey" placeholder="Enter Twitter App Key" data-bind="value: twitterAppKey">
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">
                                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Your Linkedin Page URL - e.g. https://www.linkedin.com/" data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>
                                                </div>
                                                <label for="txtLinkedInURL" class="control-label text-right tooltip-float">Linkedin URL</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" class="form-control" id="txtLinkedInURL" placeholder="Enter Linkedin URL" data-bind="value: linkedinUrl">
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">
                                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Enter corporate home title" data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>
                                                </div>
                                                <label for="txtCorpHomeURL" class="control-label text-right tooltip-float">Home URL</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" class="form-control" id="txtCorpHomeURL" placeholder="Enter Corporate Home URL" data-bind="value: url">
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane fade" id="tab-territoryForCrm">
                                <div class="col-lg-12">
                                    <!--ko if: type() != 0 && type() != 1 && type() != 2-->
                                    <div class="col-lg-12">
                                        <h3 class="group-header">Territories</h3>
                                        <div class="col-lg-12 clearfix">
                                            <header class="main-box-header clearfix">
                                                <div class="filter-block pull-right">
                                                    <div class="form-group pull-left">
                                                        <input type="text" class="form-control" placeholder="Search..." data-bind="value: $root.searchCompanyTerritoryFilter, validationOnElement: $root.searchCompanyTerritoryFilter">
                                                        <i class="fa fa-search search-icon" data-bind="click:$root.searchCompanyTerritory"></i>
                                                    </div>
                                                    <a data-toggle="modal" class="btn btn-primary pull-right" data-bind="click: $root.onCreateNewCompanyTerritory">
                                                        <i class="fa fa-plus-circle fa-lg"></i>&nbsp;Add
                                                    </a>
                                                </div>
                                            </header>
                                            <div class="main-box-body clearfix">
                                                <div class="table-responsive">
                                                    <table class="table table-striped table-hover">
                                                        <thead>
                                                            <tr>
                                                                <th><span>Territory</span></th>
                                                                <th><span>Code</span></th>
                                                                <th>&nbsp;</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <!-- ko template: { name: 'itemCompanyTerritoryTemplate' , foreach: $root.selectedStore().companyTerritories } -->
                                                            <!-- /ko -->
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                            <footer class="main-box-header pull-right">
                                                <div id="pagerDiv">
                                                    <!-- ko with: $root.companyTerritoryPager -->
                                                    @Html.Partial("~/Areas/Common/Views/Shared/_Pager.cshtml")
                                                    <!-- /ko -->
                                                </div>
                                            </footer>
                                        </div>
                                    </div>
                                    <!--/ko-->
                                    <h3 class="group-header">Addresses</h3>
                                    <div class="col-lg-12 clearfix">
                                        <header class="main-box-header clearfix">
                                            <div class="filter-block">
                                                <div class="filter-block pull-right col-xs-5">
                                                    <div class="form-group pull-left col-xs-8" style="padding-right: 0px; margin-right: 0px;">
                                                        <input type="text" class="form-control " placeholder="Search ..." data-bind="value: $root.searchAddressFilter, validationOnElement: $root.searchAddressFilter">
