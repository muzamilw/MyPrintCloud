
<div class="modal fade my-modal" id="myAddressSetModalForCrm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" data-backdrop="static" aria-hidden="true">
    <div style="width: 838px;" class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">Add/Edit Address</h4>
            </div>
            <div class="clearfix">
            </div>
            <br />
            <div class=" form-horizontal clearfix" data-bind="with: $root.selectedAddress">
                <div class="col-lg-5">
                    <div class="form-group ">
                        <div class="col-lg-5 text-right">
                             <label for="txtAddressName" class="control-label text-right tooltip-float">Address Name</label>
                        </div>
                        <div class="col-lg-7">
                            <input type="text" class="form-control" id="txtAddressName" data-bind="value: addressName, validationToolTip: addressName">
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-lg-5 text-right">
                            <label for="txtAdd1" class="control-label text-right tooltip-float">Address 1</label>
                        </div>
                        <div class="col-lg-7">
                            <input type="text" class="form-control" id="txtAdd1" data-bind="value: address1, validationToolTip: address1">
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-lg-5 text-right">
                           <label for="txtAdd2" class="control-label text-right tooltip-float">Address 2</label>
                        </div>
                        <div class="col-lg-7">
                            <input type="text" class="form-control" id="txtAdd2" data-bind="value: address2">
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-lg-5 text-right">
                           <label for="txtAdd3" class="control-label text-right tooltip-float">Address 3</label>
                        </div>
                        <div class="col-lg-7">
                            <input type="text" class="form-control" id="txtAdd3" data-bind="value: address3">
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-lg-5 text-right">
                           <label for="txtCountry" class="control-label text-right tooltip-float">Country</label>
                        </div>
                        <div class="col-lg-7">
                            <select id="country" class="form-control" data-bind="options: $root.countries , optionsText: 'CountryName', optionsValue: 'CountryId' ,value:country,validationtooltip:country"></select>
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-lg-5 text-right">
                            <label for="txtState" class="control-label text-right tooltip-float">State</label>
                        </div>
                        <div class="col-lg-7">
                            <select id="state" class="form-control" data-bind="options: $root.filteredStates  , optionsText: 'StateName', optionsValue: 'StateId' ,value:state,validationtooltip:state"></select>
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-lg-5 text-right">
                            <label for="txtCity" class="control-label text-right tooltip-float">City</label>
                        </div>
                        <div class="col-lg-7">
                            <input type="text" class="form-control" id="txtCity" data-bind="value: city, validationToolTip: city">
                        </div>
                    </div>

                    <div class="form-group ">
                        <div class="col-lg-5 text-right">
                           <label for="txtPostalCode" class="control-label text-right tooltip-float">Postal Code</label>
                        </div>
                        <div class="col-lg-7">
                            <input type="text" class="form-control" id="txtPostalCode" data-bind="value: postCode">
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-lg-5 text-right">
                            <label for="txtEmail" class="control-label text-right tooltip-float">Email</label>
                        </div>
                        <div class="col-lg-7">
                            <input type="text" class="form-control" id="txtEmail" data-bind="value: email">
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-lg-5 text-right">
                           <label for="txtFAQ" class="control-label text-right tooltip-float">FAQ</label>
                        </div>
                        <div class="col-lg-7">
                            <input type="text" class="form-control" id="txtFAQ" data-bind="value: fAO">
                        </div>
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group">
                        <div class="col-lg-5 text-right">
                           <label for="txtTel1" class="control-label text-right tooltip-float">Tel 1</label>
                        </div>
                        <div class="col-lg-3">
                            <input style="width:131px;padding: 5px;" type="text" class="form-control" id="txtTel1" data-bind="value: tel1">
                        </div>
                        <div class="col-lg-2 text-right">
                            <label for="txtExt1" class="control-label text-right tooltip-float">Ext 1</label>
                        </div>
                        <div class="col-lg-2">
                            <input  type="text" class="form-control" id="txtExt1" data-bind="value: extension1">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-lg-5 text-right">
                           <label for="txtTel2" class="control-label text-right tooltip-float">Tel 2</label>
                        </div>
                        <div class="col-lg-3">
                            <input style="width:131px;padding: 5px;" type="text" class="form-control" id="txtTel2" data-bind="value: tel2">
                        </div>
                        <div class="col-lg-2 text-right">
                            <label for="txtExt2" class="control-label text-right tooltip-float">Ext 2</label>
                        </div>
                        <div class="col-lg-2">
                            <input type="text" class="form-control" id="txtExt2" data-bind="value: extension2">
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-lg-5 text-right">
                            <label for="txtReference" class="control-label text-right tooltip-float">Reference</label>
                        </div>
                        <div class="col-lg-7">
                            <input type="text" class="form-control" id="txtReference" data-bind="value: reference">
                        </div>
                    </div>
                    <!--ko if: $root.selectedStore().type() != 0 && $root.selectedStore().type() != 1 && $root.selectedStore().type() != 2-->
                    <div class="form-group ">
                        <div class="col-lg-5 text-right">
                            
                            <label for="txtReference" class="control-label text-right tooltip-float">Territory</label>
                        </div>
                        <div class="col-lg-7">
                            <select class="form-control col-lg-4" data-bind="options:$root.addressCompanyTerritoriesFilter,optionsText:'territoryName',optionsValue:'territoryId', value: territoryId"></select> @*,optionsCaption: 'Choose...'*@
                        </div>
                    </div>
                    <!--/ko-->
                    <div class="form-group ">
                        <div class="col-lg-5 text-right">
                            

                            <label for="txtFax" class="control-label text-right tooltip-float">Fax</label>
                        </div>
                        <div class="col-lg-7">
                            <input type="text" class="form-control" id="txtFax" data-bind="value: fax">
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-lg-5 text-right">
                            <div class="bs-example-tooltips tooltip-float-width">
                                <a class="input-group-addon" tabindex="-1" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-content="Latitude" data-container="body" data-toggle="popover" data-placement="right" data-original-title="" title="">
                                    <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                </a>
                            </div>

                            <label for="txtGeoLatitude" class="control-label text-right tooltip-float">Latitude</label>
                        </div>
                        <div class="col-lg-7">
                            <input type="text" class="form-control" id="txtGeoLatitude" data-bind="value: geoLatitude">
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-lg-5 text-right">
                            <div class="bs-example-tooltips tooltip-float-width">
                                <a class="input-group-addon" tabindex="-1" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-content="Longitude" data-container="body" data-toggle="popover" data-placement="right" data-original-title="" title="">
                                    <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                </a>
                            </div>

                            <label for="txtGeoLongitude" class="control-label text-right tooltip-float">Longitude</label>
                        </div>
                        <div class="col-lg-7">
                            <input type="text" class="form-control" id="txtGeoLongitude" data-bind="value: geoLongitude">
                        </div>
                    </div>
                    <!--ko if: $root.selectedStore().type() != 0-->
                    @*<div class="form-group">
                        <label class="col-md-9 text-right" for="switchTerritoryDBillAdd">&nbsp;&nbsp;Territory Default Billing Address</label>
                        <div class="col-md-3">
                            <div class="onoffswitch">
                                <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="switchTerritoryDBillAdd" data-bind="checked: isDefaultTerrorityBilling">
                                <label class="onoffswitch-label" for="switchTerritoryDBillAdd">
                                    <div class="onoffswitch-inner"></div>
                                    <div class="onoffswitch-switch"></div>
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-9 text-right" for="switchTerritoryDShippAdd">&nbsp;&nbsp;Territory Default Shipping Address</label>
                        <div class="col-md-3">
                            <div class="onoffswitch">
                                <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="switchTerritoryDShippAdd" data-bind="checked: isDefaultTerrorityShipping">
                                <label class="onoffswitch-label" for="switchTerritoryDShippAdd">
                                    <div class="onoffswitch-inner"></div>
                                    <div class="onoffswitch-switch"></div>
                                </label>
                            </div>
                        </div>
                    </div>*@
                    <!--/ko-->
                    <div class="form-group">
                        <label class="col-md-9 text-right" for="switchCompDAdd">&nbsp;&nbsp;Company Default Address</label>
                        <div class="col-md-3">
                            <div class="checkbox-nice">
                                <input type="checkbox" name="onoffswitch" id="switchCompDAdd" data-bind="checked: isDefaultAddress">
                                <label  for="switchCompDAdd">
                                    
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-9 text-right" for="switchCompDShippAdd">&nbsp;&nbsp;Company Default Shipping Address</label>
                        <div class="col-md-3">
                            <div class="checkbox-nice">
                                <input type="checkbox" name="onoffswitch"  id="switchCompDShippAdd" data-bind="checked: isDefaultShippingAddress">
                                <label  for="switchCompDShippAdd">
                                    
                                </label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer col-xs-12">
                    <button type="button" class="btn btn-danger archiveButton" data-bind="click:$root.onDeleteAddress, enable: $root.selectedAddress().addressId() != undefined">Archive</button>
                    <button type="button" class="btn btn-primary" data-bind="click: $root.onSaveAddress">Save</button>
                    <button type="button" class="btn btn-default" data-bind="click: $root.onCloseAddress" style="margin-left:0px;">Close</button>
                </div>
            </div>
        </div>
    </div>
</div>
@* Wizard Popup ends here*@
