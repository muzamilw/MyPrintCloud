
<div class="modal fade" id="paperSheetDialog" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div style="width:838px;" class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">Add/Edit Paper Sheet</h4>
            </div>
            <div class="clearfix">
            </div>
            <br />
            <div class=" form-horizontal clearfix" data-bind="with: $root.selectedPaperSheet">
                <header class="main-box-header clearfix"></header>
                <div class="form-horizontal clearfix">
                    <div class="col-lg-6">
                        <div class="form-group ">
                            <div class="col-lg-5 text-right">
                                <div class="bs-example-tooltips tooltip-float-width">
                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-content="Name of Paper Sheet" data-container="body" data-toggle="popover" data-placement="right" data-original-title="" title="">
                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                    </a>
                                </div>
                                <label class="control-label text-right tooltip-float">Name</label>
                            </div>

                            <div class="col-lg-7">
                                <input type="text" class="form-control" data-bind="value: name, validationTooltip: name" />
                            </div>
                        </div>
                        <div class="form-group ">
                            <div class="col-lg-5 text-right">
                                <div class="bs-example-tooltips tooltip-float-width">
                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-content="Default Flag for Paper Sheet" data-container="body" data-toggle="popover" data-placement="right" data-original-title="" title="">
                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                    </a>
                                </div>
                                <label class="control-label text-right tooltip-float">Is Default</label>
                            </div>

                            <div class="col-lg-7">
                                <div class="checkbox-nice">
                                    <input type="checkbox" data-bind="checked: isArchived" id="isArchived">
                                    <label for="isArchived"></label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group">
                            <div class="col-lg-3 text-right">
                                <div class="bs-example-tooltips tooltip-float-width">
                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-content="Height of Paper Sheet" data-container="body" data-toggle="popover" data-placement="right" data-original-title="" title="">
                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                    </a>
                                </div>
                                <label class="control-label text-right tooltip-float">Height</label>
                            </div>
                            <div class="col-lg-6">
                                <input type="text" class="form-control" data-bind="value: height, validationTooltip: height" />
                            </div>
                            <div class="col-lg-2">
                                ( <label class="control-label" data-bind="text:$root.lengthUnit"></label> )
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-lg-3 text-right">
                                <div class="bs-example-tooltips tooltip-float-width">
                                    <a class="input-group-addon" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-content="Width of Paper Sheet" data-container="body" data-toggle="popover" data-placement="right" data-original-title="" title="">
                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                    </a>
                                </div>
                                <label class="control-label text-right tooltip-float">Width</label>
                            </div>
                            <div class="col-lg-6">
                                <input type="text" class="form-control" data-bind="value: width, validationTooltip: width" />
                            </div>
                            <div class="col-lg-2">
                                ( <label class="control-label" data-bind="text:$root.lengthUnit"></label> )
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="modal-footer" data-bind="with: $root.selectedPaperSheet">
                <button type="button" class="btn btn-primary" data-bind="click: $root.savePaperSheet, css: { 'btn-warning': hasChanges }, enable: hasChanges">Save</button>
                <button type="button" class="btn btn-default removeLeftMargin" data-bind="click: $root.onClosePaperSheet">Close</button>
            </div>
        </div>
    </div>
</div>
