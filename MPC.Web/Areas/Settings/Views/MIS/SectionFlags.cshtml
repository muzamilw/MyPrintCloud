<script src="~/Scripts/Ace/colorpicker.js"></script>
<script>
    $(document).ready(function () {
        var ColorPickerId;
        $(".Color").click(function () {
            ColorPickerId = this.id;
            $("#" + ColorPickerId).ColorPicker
            ({
                color: '',
                onShow: function (colpkr) {
                    $(colpkr).fadeIn(500);
                    return false;
                },
                onHide: function (colpkr) {
                    $(colpkr).fadeOut(500);
                    return false;
                },
                onChange: function (hsb, hex, rgb) {
                    $("#" + ColorPickerId).css("background-color", "#" + hex);
                }
            });
        });


    });



</script>
