    <div class="form-group">
        <div class="col-lg-5 text-right col-padding">
          
        </div>
        <div class="col-lg-7">
            <div class="table-responsive" style="margin-right:12px;">
                <a href="#" class="btn btn-primary pull-right" data-bind="click:$root.AddGuiltineLookup" style="margin-top:25px;">
                    Add Cut Line
                </a>
                <table id="chartOfAccountTablle" class="table table-striped table-hover">
                    <thead>
                        <tr>
                            <td align="left" class="text-style">
                                SECTIONS
                            </td>
                            <td align="left" class="text-style">
                                UPS
                            </td>
                            <td align="left" class="text-style">
                                CUTS WITHOUT GUTTERS
                            </td>
                            <td align="left" class="text-style">
                                CUTS WITH GUTTERS
                            </td>
                        </tr>
                    </thead>

                    <!--ko template: {name: 'GuillotinePTVTemplate' , foreach: $root.selectedGuillotineClickCharge().GuillotinePTVList} -->
                    <!-- /ko -->

                </table>
            </div>
            </div>
     </div>
           
        </div>
        <script type="text/html" id="GuillotinePTVTemplate">
            <tr>
                <td style="width:100px;">
                    <input type="text" class="form-control" data-bind="autoNumeric: NoofSections , settings:{mDec:0}">
                </td>
                <td style="width:70px;">
                    <input type="text" class="form-control" data-bind="autoNumeric: NoofUps , settings:{mDec:0}">
                </td>
                <td style="width:200px;">
                    <input type="text" class="form-control" data-bind="autoNumeric: Noofcutswithoutgutters , settings:{mDec:0}">
