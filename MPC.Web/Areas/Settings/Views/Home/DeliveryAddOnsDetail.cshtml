@{
    ViewBag.Title = "Add/Edit Delivery AddOns ";
}
@section leftmenue {
}
<div class="row" data-bind="visible: $root.isEditorVisible()" style="display: none">
    <div class="col-lg-12 clearfix" data-bind="with: selectedCostCenter()">
        <div class="row">
            <div class="col-lg-12">
                <ol class="breadcrumb" style="padding-left:8px;">
                    <li><a href="~/Dashboard/Index">Home</a></li>
                    <li><a href="/mis/Settings" class="my-navigator">Settings</a></li>
                    <li><a href="~/Settings/Home/DeliveryCostCentres">Delivery Cost Centers</a></li>
                    <li class="active"><span>Shipping Cost Center</span></li>
                </ol>

            </div>
        </div>
        <div class="row" id="user-profile">
            <div>
                <h1 class="pull-left" style="min-height: 30px;" data-bind="text: name()"></h1>
            </div>
            <div style="margin-top:42px;">
                <div class="pull-right" style="margin-right:15px;">
                    <button type="button" class="btn btn-primary" data-bind="click: $root.saveCostCenter, enable: hasChanges, css: { 'btn-warning': hasChanges }">
                        &nbsp;Save
                    </button>
                    <button type="button" class="btn btn-primary" data-bind="click: $root.closeCostCenterDetail">
                        &nbsp;Close
                    </button>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12" style="padding-left:17px;padding-right:17px;margin-top:28px;">
                <div class="main-box clearfix">
                    <div class="tabs-wrapper profile-tabs">
                        <ul class="nav nav-tabs">
                            <li class="active"><a href="#tab-CCDescription" data-toggle="tab">Cost Center Description</a></li>

                            @*<li><a href="#tab-DeliveryTimeZone" data-toggle="tab">Delivery Zone Table</a></li>*@
                        </ul>
                        <div class="tab-content">
                            <div class="tab-pane fade in active" id="tab-CCDescription">
                                <div class="col-lg-12">
                                    <div class=" form-horizontal clearfix">

                                        <div class="col-lg-6">
                                            <div class="form-group ">
                                                <div class="col-lg-5 text-right">

                                                    

                                                </div>

                                                <div class="col-lg-7">
                                                    <input type="text" class="form-control font24px height60" id="txtEmail" placeholder="Enter Cost Center name" data-bind="value: name, validationTooltip: name">
                                                </div>
                                            </div>

                                            <div class="form-group ">
                                                <div class="col-lg-5 text-right">

                                                    <div class="bs-example-tooltips tooltip-float-width">


                                                        </div>


                                                    <label for="txtWebDesc" class="control-label text-right tooltip-float">Description</label>

                                                </div>

                                                <div class="col-lg-7">
                                                    <textarea class="form-control" id="txtWebDesc" placeholder="Enter Web Store Description" rows="5" data-bind="value: webStoreDesc"></textarea>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <div class="col-lg-5 text-right">

                                                    <div class="bs-example-tooltips tooltip-float-width">

                                                        <a class="input-group-addon" tabindex="-1" style="text-decoration: none; border: 0; cursor: pointer;" data-content="You can Select UPS, Fedex or your own shipping carrier. In case of Fedex you will need to select the Fedex service type also. For both Fedex and UPS, shipping price and time will be calculated when user selects his/her shipping address and goes to confirmation screen by calling the respective service. In case of other (your own) you will need to setup the shipping price and time which will be display on store’s checkout area." data-container="body" data-toggle="popover" data-placement="right" data-original-title="" title="">
                                                            <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                        </a>

                                                    </div>


                                                    <label for="ddCarrier" class="control-label text-right tooltip-float">Send & Calculate Shipping Via</label>

                                                </div>

                                                <div class="col-lg-7">

                                                    <select class="form-control" data-bind="options:$root.deliveryCarriers, optionsText:'CarrierName',optionsValue:'CarrierId', value: carrierId"></select>

                                                </div>

                                            </div>

                                            <div class="form-group" data-bind="visible: carrierId() === 1">
                                                <div class="col-lg-5 text-right">
                                                    <div class="bs-example-tooltips tooltip-float-width">


                                                    </div>
                                                    
                                                    <label for="ddCarrier" class="control-label text-right tooltip-float">Fedex Service Type</label>

                                                </div>

                                                <div class="col-lg-7">
                                                    <select class="form-control" data-bind="options:$root.fedexServiceTypes, optionsText:'Text',optionsValue:'Text' ,value:deliveryServiceType"></select>
                                                </div>
                                            </div>

                                            <div class="form-group" data-bind="visible: carrierId() != 1">
                                                <div class="col-lg-5 text-right">

                                                    <div class="bs-example-tooltips tooltip-float-width">

                                                    </div>


                                                    <label for="txtDelcharges" class="control-label text-right tooltip-float">Fixed Shipping Price</label>

                                                </div>
                                                <div>
                                                    <div class="col-md-4 col-lg-4 input-group" style="padding-left: 8px;">
                                                        <span class="input-group-addon" data-bind="text:$root.CurrencySymbol"></span>
                                                        <input type="text" class="form-control text-right" data-bind="autoNumeric:deliveryCharges, settings:{mDec:2}">
                                                    </div>

                                                </div>

                                            </div>
                                            <div class="form-group" data-bind="visible: carrierId() != 1">
                                                <div class="col-lg-5 text-right">

                                                    <div class="bs-example-tooltips tooltip-float-width">

                                                        @*<a class="input-group-addon" tabindex="-1" style="text-decoration: none; border: 0; cursor: pointer;" data-content="Markup applied to delivery." data-container="body" data-toggle="popover" data-placement="right" data-original-title="" title="">
                                                            <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                        </a>*@

                                                    </div>


                                                    <label class="control-label text-right tooltip-float">Markup</label>

                                                </div>

                                                <div class="col-lg-4 col-md-4">

                                                    <select class="form-control" data-bind="options:$root.markups, optionsText:'MarkUpName',optionsValue:'MarkUpId', value: defaultVaId"></select>

                                                </div>

                                            </div>
                                            <div class="form-group" data-bind="visible: carrierId() != 1">
                                                <div class="col-lg-5 text-right">

                                                    <div class="bs-example-tooltips tooltip-float-width">
                                                    </div>
                                                    
                                                    <label for="txtDelvTime" class="control-label text-right tooltip-float">Fixed Delivery Time</label>
                                                </div>
                                                <div>
                                                    <div class="col-md-4 col-lg-4" style="padding-right:0px;">
                                                        <input type="text" class="form-control text-right" data-bind="value:estimateProductionTime">
                                                    </div>
                                                    <div class="col-md-2">
                                                        <span class="help-block" style=" margin-top: 8px;">Day(s)</span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <div class="col-lg-5 text-right">
                                                    <div class="bs-example-tooltips tooltip-float-width">
                                                    </div>
                                                    <label for="switchisDisabled" class="control-label text-right tooltip-float">Is Cost Center enable</label>
                                                </div>
                                                <div class="col-lg-7">
                                                    <div class="onoffswitch">
                                                        <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="switchisDisabled" data-bind="checked:isDisabled">
                                                        <label class="onoffswitch-label" for="switchisDisabled">
                                                            <div class="onoffswitch-inner"></div>
                                                            <div class="onoffswitch-switch"></div>
                                                        </label>
                                                    </div>
                                                    <span class="help-block text-right"></span>
                                                </div>
                                            </div>
                                            <div class="form-group">

                                                <div class="col-lg-5 text-right">
                                                    <div class="bs-example-tooltips tooltip-float-width">
                                                    </div>
                                                    <label for="switchIsPublished" class="control-label text-right tooltip-float">Is Cost Center Published on Webstore</label>

                                                </div>
                                                <div class="col-lg-3">
                                                    <div class="onoffswitch">
                                                        <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="switchIsPublished" data-bind="checked:isPublished">
                                                        <label class="onoffswitch-label" for="switchIsPublished">
                                                            <div class="onoffswitch-inner"></div>
                                                            <div class="onoffswitch-switch"></div>
                                                        </label>
                                                    </div>
                                                    <span class="help-block text-right"></span>
                                                </div>

                                            </div>
