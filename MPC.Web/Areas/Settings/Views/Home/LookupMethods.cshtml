@{
    ViewBag.Title = "Lookup Methods";
}
@section leftmenue {

}
<script src="http://mpc/Scripts/jquery.min.js"></script>
<script src="http://mpc/Scripts/jquery-1.10.2.js"></script>
<script src="~/Content/themes/Centaurus/js/jquery.magnific-popup.min.js"></script>



<script>
    $(document).ready(function () {

        $("ol#treeviewSpeedWeight").hide();
        $("ol#treeviewPerHour").hide();
        $("ol#treeviewClickChargeZone").hide();
        $("ol#treeviewMeterCharge").hide();
        $("ol#treeviewGuillotine").hide();


        $(".treeviewSpeedWeight-partial").hide();
        $(".treeviewPerHour-partial").hide();
        $(".treeviewMeterCharge-partial").hide();
        $(".treeviewClickChargeZone-partial").hide();
        $(".treeviewGuillotine-partial").hide();

        //$("li").click(function () {
        //    var GettingId = this.id;
        //    $("ol" + "#" + GettingId).toggle();
        //});
        //$("ol").click(function () {
        //    var GettingId = this.id;
        //    $("div[class*='-partial']").hide();
        //    $("." + GettingId + "-partial").show();
        //    return false;
        //});

        $('.bs-example-tooltips a').popover();
    });
</script>
<style>
    .treeviewSub {
        list-style-type: none;
    }

    li {
        cursor: pointer;
    }
</style>
<div class="row" id="divlookupMethodBinding">
    <div class="col-lg-12" data-bind="visible: !isEditorVisible()">
        @*<div class="col-lg-12" >*@
        <div class="row">
            <div class="col-lg-12">
                <ol class="breadcrumb">
                    <li><a href="~/Dashboard/Index">Settings</a></li>
                    <li>Home</li>

                    <li class="active"><span>Lookup Methods</span></li>
                </ol>
                <div class="clearfix">
                    <h1 class="pull-left">Machine Lookup Methods</h1>
                    <div>
                        <table id="table">
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <div class="row" id="prefixesdata" style="float:right;">
            <div class="input-group">
                <button class="btn  btn-info btn-primary" style="float: right;margin-bottom: 10px;margin-right: 15px;display:inline;" id="saveBtn">Save</button>
                <button type="button" class="btn btn-primary dropdown-toggle" data-toggle="dropdown">Add Lookup <span class="caret"></span></button>
                <ul class="dropdown-menu">
                    <li><a href="#">Click Charge</a></li>
                    <li><a href="#">Speed Weight</a></li>
                    <li><a href="#">Per Hour</a></li>
                    <li><a href="#">Click Charge Zones</a></li>
                    <li><a href="#">Guillotine Click Charge</a></li>
                    <li><a href="#">Meter Per Hour Meter Charge</a></li>
                </ul>
                &nbsp;&nbsp;&nbsp;
                <button class="btn  btn-info btn-primary" style="float: right;margin-bottom: 10px;margin-right: 15px;display:inline;" id="saveBtn">Delete Lookup</button>
            </div>
        </div>
        <div class="col-lg-12 main-box clearfix">
            <div class="main-box-body">
                <div class="col-lg-12 form-horizontal" style="padding-top:40px;padding-bottom:40px;">
                    <div class="col-md-3" style="width:auto;">

                        @*Div For TreeView*@
                        <div style="display:inline;position:relative;">
                            <ol class="dd-list">
                                <li class="dd-item dd-item-list" id="treeviewClickCharge">
                                    <div class="dd-handle-list"><i class="fa fa-bars"></i></div>
                                    <div class="dd-handle" data-bind="text:lookupClickCharge">
                                    </div>
                                    <!-- ko template: { name: 'lookupClickChargeTemplate' , foreach: lookupClickChargeList } -->
                                    <!-- /ko -->

                                </li>
                            </ol>

                            <ol class="dd-list">
                                <li class="dd-item dd-item-list" id="treeviewSpeedWeight">
                                    <div class="dd-handle-list"><i class="fa fa-bars"></i></div>
                                    <div class="dd-handle" data-bind="text:lookupSpeedWeight"></div>
                                    <!-- ko template: { name: 'lookupSpeedWeightTemplate' , foreach: lookupClickChargeList } -->
                                    <!-- /ko -->

                                </li>
                            </ol>

                            <ol class="dd-list">
                                <li class="dd-item dd-item-list" id="treeviewPerHour">
                                    <div class="dd-handle-list"><i class="fa fa-bars"></i></div>
                                    <div class="dd-handle" data-bind="text:lookupPerHour"></div>
                                    <ol class="dd-list" id="treeviewPerHour">
                                        <li class="dd-item dd-item-list">
                                            <div class="dd-handle-list"><i class="fa fa-bars"></i></div>
                                            <div class="dd-handle">Click Charge Zone 2</div>
                                        </li>
                                    </ol>
                                </li>
                            </ol>


                            <ol class="dd-list">
                                <li class="dd-item dd-item-list" id="treeviewClickChargeZone">
                                    <div class="dd-handle-list"><i class="fa fa-bars"></i></div>
                                    <div class="dd-handle" data-bind="text:lookupClickChargeZones"></div>
                                    <ol class="dd-list" id="treeviewClickChargeZone">
                                        <li class="dd-item dd-item-list">
                                            <div class="dd-handle-list"><i class="fa fa-bars"></i></div>
                                            <div class="dd-handle">Konica Zone</div>
                                        </li>
                                    </ol>
                                </li>
                            </ol>


                            <ol class="dd-list">
                                <li class="dd-item dd-item-list" id="treeviewGuillotine">
                                    <div class="dd-handle-list"><i class="fa fa-bars"></i></div>
                                    <div class="dd-handle" data-bind="text:lookupGuillotineClickCharge"></div>
                                    <ol class="dd-list" id="treeviewGuillotine">
                                        <li class="dd-item dd-item-list">
                                            <div class="dd-handle-list"><i class="fa fa-bars"></i></div>
                                            <div class="dd-handle">Guillotine LookUp</div>
                                        </li>
                                    </ol>
                                </li>
                            </ol>

                            <ol class="dd-list">
                                <li class="dd-item dd-item-list" id="treeviewMeterCharge">
                                    <div class="dd-handle-list"><i class="fa fa-bars"></i></div>
                                    <div class="dd-handle" data-bind="text:lookupMeterPerHourClickCharge"></div>
                                    <ol class="dd-list" id="treeviewMeterCharge">
                                        <li class="dd-item dd-item-list">
                                            <div class="dd-handle-list"><i class="fa fa-bars"></i></div>
                                            <div class="dd-handle">Meter Per Hour</div>
                                        </li>
                                    </ol>
                                </li>
                            </ol>
                        </div>
                    </div>

                    <div style="display:inline;margin-left:10px;width:73% !important;" class="col-md-8">
                        <div class="treeviewClickCharge-partial">
                            @Html.Partial("_ClickChargeLookUp")
                        </div>
                        <div class="treeviewSpeedWeight-partial">
                            @Html.Partial("_SpeedWeight")
                        </div>
                        <div class="treeviewPerHour-partial">
                            @Html.Partial("_PerHour")
                        </div>
                        <div class="treeviewMeterCharge-partial">
                            @Html.Partial("_MeterPerHour")
                        </div>
                        <div class="treeviewGuillotine-partial">
                            @Html.Partial("_GuillotineClickCharge")
                        </div>
                        <div class="treeviewClickChargeZone-partial">
                            @Html.Partial("_ClickChargeZone")
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>
<script id="lookupClickChargeTemplate" type="text/html">
    <ol class="dd-list" id="treeviewClickCharge">
        <li class="dd-item dd-item-list">
            <div class="dd-handle-list"><i class="fa fa-bars"></i></div>
            <div class="dd-handle" data-bind="text:Name, click:$root.GetClickChargeById"></div>
        </li>
    </ol>

</script>
<script id="lookupSpeedWeightTemplate" type="text/html">


    <ol class="dd-list" id="treeviewSpeedWeight">
        <li class="dd-item dd-item-list">
            <div class="dd-handle-list"><i class="fa fa-bars"></i></div>
            <div class="dd-handle" data-bind="text:Name"></div>
        </li>
    </ol>
</script>

@section Scripts{
    <script src="~/Scripts/jquery.form.js"></script>

    <script>
        require(["lookupMethods/lookupMethods.view", "common/confirmation.view"]);

        $(function () {
            $('#lisettings').addClass('active');
            $('#limissettings').addClass('active');
            $('#liLookup').addClass('active');

        });


    </script>
}

