@{
    ViewBag.Title = "Delivery Carrier";
}
<div class="row">
    <div class="col-lg-12">
        <div class="row">
            <div class="col-lg-12">
                <ol class="breadcrumb">
                    <li><a href="~/Dashboard/Index">Home</a></li>
                    <li class="active">Delivery Cost Centers</li>
                    
                </ol>
                <h1>Delivery Carriers</h1>
            </div>
        </div>


        <div class="col-lg-12">
            <div class="pull-right">
                <button id="open-wizard" data-bind="click: createOrder" class="btn btn-primary pull-right">
                    <i class="fa fa-plus-circle fa-lg"></i>&nbsp;Add Delivery Carrier
                </button>
            </div>
        </div>

        <div>
            &nbsp;
        </div>

        <div class="row" id="user-profile">


            <div class="col-lg-12 form-horizontal ">
                <div class="main-box no-header clearfix">
                    <div class="main-box-body clearfix">
                        <div class="table-responsive">
                            <table class="table table-striped table-hover">
                                <thead>
                                    <tr>
                                        <th><a href="#"><span>Name</span></a></th>
                                        <th><a href="#"><span>URL</span></a></th>
                                        <th><a href="#"><span>API Key</span></a></th>
                                        <th>Flag</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>
                                            <a data-toggle="modal" href="#myAddEditDelvCarrierModal" class="table-link">FedEx</a>
                                        </td>
                                        <td>
                                            http://www.google.com.pk
                                        </td>
                                        <td>
                                            46353535345
                                        </td>
                                        <td>
                                            <span class="label label-success" data-bind="style: { background: flagColor }" style="background: red;"><i class="fa fa-flag"></i></span>
                                        </td>
                                       
                                    </tr>
                                    
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@Html.Partial("_AddEditDeliveryCarriers")
