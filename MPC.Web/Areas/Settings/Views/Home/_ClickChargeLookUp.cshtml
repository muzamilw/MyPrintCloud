<style>
   
    .aligndivs
    {
        display:inline;
        float:left;
        
        margin-right:10px;
        margin-bottom:10px;
    }
</style>
@*with: selectedClickCharge()*@

<div data-bind="visible: $root.isClickChargeEditorVisible(),with: selectedClickCharge()" style="display: none">
    


        <div>
            <h3 style="padding-right:10px;margin:0px;">Specify values for the click charge Lookup Calculation</h3>
        </div>
        <br />
        <div class="form-group">
            <div class="col-lg-5 text-right">
                <div class="bs-example-tooltips tooltip-float-width">
                    <a class="input-group-addon" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Enter the name of the Lookup method" data-original-title="" title="">
                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                    </a>
                </div>
                <label for="txtCompanyName" class="control-label text-right tooltip-float">Enter the name of the Lookup method</label>
            </div>
            <div class="col-lg-6">
                <input type="text" name="Store Name" class="form-control" id="txtCompanyName" placeholder="Click Chnage Lookup">
            </div>
        </div>

        <div class="form-group">
            <div class="col-lg-5 text-right">
                <div class="bs-example-tooltips tooltip-float-width">
                    <a class="input-group-addon" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Enter the number of chargeable sheets" data-original-title="" title="">
                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                    </a>
                </div>
                <label for="txtCompanyName" class="control-label text-right tooltip-float">Enter the number of chargeable sheets</label>
            </div>
            <div class="col-lg-6">
                <input type="text" class="form-control" id="txtAddressLine1" placeholder="Chargeable Sheets">
            </div>
        </div>

        <div class="form-group">
            <div class="col-lg-5 text-right">
                <div class="bs-example-tooltips tooltip-float-width">
                    <a class="input-group-addon" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Enter Cost of the chargeable sheets" data-original-title="" title="">
                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                    </a>
                </div>
                <label for="txtCompanyName" class="control-label text-right tooltip-float">Enter Cost of the chargeable sheets</label>
            </div>
            <div class="col-lg-6">

                <div class="input-group">
                    <span class="input-group-addon">$</span>
                    <input type="text" class="form-control" id="txtAddressLine1" placeholder="Cost Chargeable Sheet">
                    <span class="input-group-addon">.00</span>
                </div>


            </div>
        </div>

        <div class="form-group">
            <div class="col-lg-5 text-right">
                <div class="bs-example-tooltips tooltip-float-width">
                    <a class="input-group-addon" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Enter Price of the chargeable sheets" data-original-title="" title="">
                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                    </a>
                </div>
                <label for="txtCompanyName" class="control-label text-right tooltip-float">Enter Price of the chargeable sheets</label>
            </div>
            <div class="col-lg-6">

                <div class="input-group">
                    <span class="input-group-addon">$</span>
                    <input type="text" class="form-control" id="txtAddressLine1" placeholder="Price Chargeable Sheet">
                    <span class="input-group-addon">.00</span>
                </div>


            </div>


        </div>
    </div>
