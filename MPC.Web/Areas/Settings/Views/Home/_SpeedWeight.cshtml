

<div data-bind="visible: $root.isSpeedWeightVisible()">

    <div>
        <h3 style="padding-right:10px;margin:0px;">Specify values of the speed weight calculation</h3>
    </div>
    <br />
    @*<div class="form-group">
            <div class="col-lg-6 text-right">
                <div class="bs-example-tooltips tooltip-float-width">
                    <a class="input-group-addon" tabindex="-1" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Enter the name of the Lookup method" data-original-title="" title="">
                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                    </a>
                </div>
                <label class="control-label text-right tooltip-float">Enter the name of the Lookup method</label>
            </div>
            <div class="col-lg-6" data-bind="with:$root.selectedlookup() ">
                <input type="text" name="Store Name" class="form-control" data-bind="value:Name" placeholder="Speed Weight Lookup">

            </div>
        </div>*@

    <div class="form-group">
        <div class="row">
            <div class="col-lg-3">

            </div>
            <div class="col-lg-9">
                <label class="control-label">Quantity break</label>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-3">

            </div>
            <div class="col-lg-9">
                <div class="aligndivs">
                    <input type="text" class="form-control bkg-color" data-bind="autoNumeric: $root.selectedSpeedWeight().SheetsQty1 , settings:{mDec:0}" placeholder="0">
                </div>
                <div class="aligndivs">
                    <input type="text" class="form-control  bkg-color" data-bind="autoNumeric: $root.selectedSpeedWeight().SheetsQty2 , settings:{mDec:0}" placeholder="0">
                </div>
                <div class="aligndivs">
                    <input type="text" class="form-control bkg-color" data-bind="autoNumeric: $root.selectedSpeedWeight().SheetsQty3 , settings:{mDec:0}" placeholder="0">
                </div>
                <div class="aligndivs">
                    <input type="text" class="form-control bkg-color" data-bind="autoNumeric: $root.selectedSpeedWeight().SheetsQty4 , settings:{mDec:0}" placeholder="0">
                </div>
                <div class="aligndivs">
                    <input type="text" class="form-control bkg-color" data-bind="autoNumeric: $root.selectedSpeedWeight().SheetsQty5 , settings:{mDec:0}" placeholder="0">
                </div>
            </div>
        </div>
        @*<div class="aligndivs" style="text-align:right;width:290px;display:inline;">
                <label  class="control-label text-right tooltip-float">Quantity</label> <br />
                <span data-bind="text:$root.WeightUnit" style=" margin-bottom: -16px;margin-top: 8px;margin-right: -30px;" class="control-label text-right tooltip-float"></span>
            </div>*@



    </div>



    <div class="form-group">
        <div class="row">
            <div class="col-lg-2">

            </div>
            <div class="col-lg-1">
                <label class="control-label" data-bind="text:'Weight (' + $root.WeightUnit() + ')'"></label>
            </div>
            <div class="col-lg-1">
                <label class="control-label">Units Per Hour</label>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-2">

            </div>
            <div class="col-lg-1">
                <div class="aligndivs" style="display:inline;">
                    <input type="text" class="form-control textboxwidth bkg-color" data-bind="autoNumeric: $root.selectedSpeedWeight().SheetWeight1 , settings:{mDec:0}" placeholder="0" style="float:right;">
                </div>
            </div>
            <div class="col-lg-9">
                <div class="aligndivs">
                    <input type="text" class="form-control" data-bind="autoNumeric: $root.selectedSpeedWeight().speedqty11 , settings:{mDec:0}" placeholder="0">
                </div>
                <div class="aligndivs">
                    <input type="text" class="form-control" data-bind="autoNumeric: $root.selectedSpeedWeight().speedqty12 , settings:{mDec:0}" placeholder="0">
                </div>
                <div class="aligndivs">
                    <input type="text" class="form-control" data-bind="autoNumeric: $root.selectedSpeedWeight().speedqty13 , settings:{mDec:0}" placeholder="0">
                </div>
                <div class="aligndivs">
                    <input type="text" class="form-control" data-bind="autoNumeric: $root.selectedSpeedWeight().speedqty14 , settings:{mDec:0}" placeholder="0">
                </div>
                <div class="aligndivs">
                    <input type="text" class="form-control" data-bind="autoNumeric: $root.selectedSpeedWeight().speedqty15 , settings:{mDec:0}" placeholder="0">
                </div>
            </div>
        </div>

    </div>

    <div class="form-group">
        <div class="row">
            <div class="col-lg-2">

            </div>
            <div class="col-lg-1">
                <div class="aligndivs" style="display:inline;text-align:right;">
                    <input type="text" class="form-control textboxwidth bkg-color" data-bind="autoNumeric: $root.selectedSpeedWeight().SheetWeight2 , settings:{mDec:0}" placeholder="0" style="float:right;">
                </div>
            </div>
            <div class="col-lg-9">
                <div class="aligndivs">
                    <input type="text" class="form-control" data-bind="autoNumeric: $root.selectedSpeedWeight().speedqty21 , settings:{mDec:0}" placeholder="0">
                </div>
                <div class="aligndivs">
                    <input type="text" class="form-control" data-bind="autoNumeric: $root.selectedSpeedWeight().speedqty22 , settings:{mDec:0}" placeholder="0">
                </div>
                <div class="aligndivs">
                    <input type="text" class="form-control" data-bind="autoNumeric: $root.selectedSpeedWeight().speedqty23 , settings:{mDec:0}" placeholder="0">
                </div>
                <div class="aligndivs">
                    <input type="text" class="form-control" data-bind="autoNumeric: $root.selectedSpeedWeight().speedqty24 , settings:{mDec:0}" placeholder="0">
                </div>
                <div class="aligndivs">
                    <input type="text" class="form-control" data-bind="autoNumeric: $root.selectedSpeedWeight().speedqty25 , settings:{mDec:0}" placeholder="0">
                </div>
            </div>
        </div>

    </div>

    <div class="form-group">
        <div class="row">
            <div class="col-lg-2">

            </div>
            <div class="col-lg-1">
                <div class="aligndivs" style="display:inline;text-align:right;">
                    <input type="text" class="form-control textboxwidth bkg-color" data-bind="autoNumeric: $root.selectedSpeedWeight().SheetWeight3 , settings:{mDec:0}" placeholder="0" style="float:right;">
                </div>
            </div>
            <div class="col-lg-9">
                <div class="aligndivs">
                    <input type="text" class="form-control" data-bind="autoNumeric: $root.selectedSpeedWeight().speedqty31 , settings:{mDec:0}" placeholder="0">
                </div>
                <div class="aligndivs">
                    <input type="text" class="form-control" data-bind="autoNumeric: $root.selectedSpeedWeight().speedqty32 , settings:{mDec:0}" placeholder="0">
                </div>
                <div class="aligndivs">
                    <input type="text" class="form-control" data-bind="autoNumeric: $root.selectedSpeedWeight().speedqty33 , settings:{mDec:0}" placeholder="0">
                </div>
                <div class="aligndivs">
                    <input type="text" class="form-control" data-bind="autoNumeric: $root.selectedSpeedWeight().speedqty34 , settings:{mDec:0}" placeholder="0">
                </div>
                <div class="aligndivs">
                    <input type="text" class="form-control" data-bind="autoNumeric: $root.selectedSpeedWeight().speedqty35 , settings:{mDec:0}" placeholder="0">
                </div>
            </div>
        </div>


    </div>

    <div class="form-group">
        <div class="col-lg-3 text-right">
            <div class="bs-example-tooltips tooltip-float-width">
                <a class="input-group-addon" tabindex="-1" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Enter hourly charge cost" data-original-title="" title="">
                    <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                </a>
            </div>
            <label for="txtCompanyName" class="control-label text-right tooltip-float">Enter hourly charge cost</label>
        </div>
        <div class="col-lg-3">

            <div class="input-group">
                <span class="input-group-addon" tabindex="-1" data-bind="text:$root.CurrencySymbol()"></span>
                <input type="text" class="form-control" data-bind="autoNumeric: $root.selectedSpeedWeight().hourlyCost , settings:{mDec:2}" placeholder="Hourly Charge Cost">

            </div>


        </div>
    </div>

    <div class="form-group">
        <div class="col-lg-3 text-right">
            <div class="bs-example-tooltips tooltip-float-width">
                <a class="input-group-addon" tabindex="-1" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="Enter hourly charge price" data-original-title="" title="">
                    <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                </a>
            </div>
            <label class="control-label text-right tooltip-float">Enter hourly charge price</label>
        </div>
        <div class="col-lg-3">

            <div class="input-group">
                <span class="input-group-addon" tabindex="-1" data-bind="text:$root.CurrencySymbol()"></span>
                <input type="text" class="form-control" data-bind="autoNumeric: $root.selectedSpeedWeight().hourlyPrice , settings:{mDec:2}" placeholder="Hourly Charge Price">

            </div>


        </div>
    </div>

</div>








