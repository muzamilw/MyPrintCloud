@{
    ViewBag.Title = "CostCentres";
}
<div class="row">
    <div class="col-lg-12">
        <div class="row">
            <div class="col-lg-12">
                <ol class="breadcrumb">
                    <li><a href="~/Dashboard/Index">Home</a></li>
                    <li class="active">Delivery Cost Centers</li>
                    
                </ol>
                <h1>Delivery Cost Centers</h1>
            </div>
        </div>
        <div class="row" id="user-profile">

            <div class="col-lg-12 form-horizontal ">
                <div class="main-box no-header clearfix">
                    <div class="main-box-body clearfix">
                        <div class="table-responsive">
                            <table class="table table-striped table-hover">
                                <thead>
                                    <tr>
                                        <th><a href="#"><span>Cost Center Name</span></a></th>
                                        <th><a href="#"><span>Cost Center Type</span></a></th>
                                        <th><a href="#"><span>Min cost</span></a></th>
                                        <th>&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>Fixed Price Delivery 
                                        </td>
                                        <td>Delivery
                                        </td>
                                        <td>$10.88
                                        </td>
                                        <td class="text-right">
                                            <a data-toggle="modal" href="~/Settings/DeliveryAddOnsDetail" class="table-link">
                                                <span class="fa-stack">
                                                    <i class="fa fa-square fa-stack-2x"></i>
                                                    <i class="fa fa-pencil fa-stack-1x fa-inverse"></i>
                                                </span>
                                            </a>
                                            <a href="#" class="table-link danger">
                                                <span class="fa-stack">
                                                    <i class="fa fa-square fa-stack-2x"></i>
                                                    <i class="fa fa-trash-o fa-stack-1x fa-inverse"></i>
                                                </span>
                                            </a>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Express Delivery
                                        </td>
                                        <td>Delivery
                                        </td>
                                        <td>$9
                                        </td>
                                        <td class="text-right">
                                            <a data-toggle="modal" href="#myDeliveryAddOnModal" class="table-link">
                                                <span class="fa-stack">
                                                    <i class="fa fa-square fa-stack-2x"></i>
                                                    <i class="fa fa-pencil fa-stack-1x fa-inverse"></i>
                                                </span>
                                            </a>
                                            <a href="#" class="table-link danger">
                                                <span class="fa-stack">
                                                    <i class="fa fa-square fa-stack-2x"></i>
                                                    <i class="fa fa-trash-o fa-stack-1x fa-inverse"></i>
                                                </span>
                                            </a>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Collect in Store 
                                        </td>
                                        <td>Delivery
                                        </td>
                                        <td>$3
                                        </td>
                                        <td class="text-right">
                                            <a data-toggle="modal" href="#myDeliveryAddOnModal" class="table-link">
                                                <span class="fa-stack">
                                                    <i class="fa fa-square fa-stack-2x"></i>
                                                    <i class="fa fa-pencil fa-stack-1x fa-inverse"></i>
                                                </span>
                                            </a>
                                            <a href="#" class="table-link danger">
                                                <span class="fa-stack">
                                                    <i class="fa fa-square fa-stack-2x"></i>
                                                    <i class="fa fa-trash-o fa-stack-1x fa-inverse"></i>
                                                </span>
                                            </a>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts {
    <script type="text/javascript">
        $(document).ready(function () {
            $('#lisettings').addClass('active');
            $('#limissettings').addClass('active');
            });
     </script>
}