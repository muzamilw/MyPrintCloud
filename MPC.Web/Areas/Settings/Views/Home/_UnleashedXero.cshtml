

<div class="main-box-body clearfix" data-bind="visible: $root.isUnleashedApiDetailVisible" style="display:none;">
    <div class="col-md-12 form-horizontal" style="margin-top:25px;">
        <div style="margin-bottom:15px;">
            <span> Configure the Unleashed API Credentials</span>
            <div style="height:20px;">
                &nbsp;
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                <div class="col-lg-5 text-right">
                    <label for="txtunleashedApiKey" class="control-label text-right tooltip-float">
                        API Id
                    </label>

                </div>

                <div class="col-lg-7">
                    <input type="text" class="form-control" id="txtunleashedApiKey" placeholder="Enter API ID" data-bind="value:unleashedApiId">
                </div>

            </div>

            <div class="form-group">
                <div class="col-lg-5 text-right">
                    <label for="txtUnleashedApiUrl" class="control-label text-right tooltip-float">
                        API Key
                    </label>
                </div>
                <div class="col-lg-7">
                    <input type="text" class="form-control" id="txtUnleashedApiUrl" placeholder="Enter API Key" data-bind="value:unleashedApiKey">
                </div>

            </div>
            
            <div class="form-group">
                <div class="col-lg-5 text-right">
                    <label for="switchunleashedActive" class="control-label text-right tooltip-float">
                        Active
                    </label>
                </div>
                <div class="col-lg-6">
                    <div class="onoffswitch">
                        <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="switchunleashedActive" data-bind="checked:isUnleashedApiActive">
                        <label class="onoffswitch-label" for="switchunleashedActive">
                            <div class="onoffswitch-inner"></div>
                            <div class="onoffswitch-switch"></div>
                        </label>
                    </div>

                </div>

            </div>

        </div>
    </div>
</div>