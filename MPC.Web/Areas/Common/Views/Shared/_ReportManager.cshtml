<div id="divReportManager" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="myModalLabel">Report Management</h4>
            </div>
            <div class="col-lg-12 main-box clearfix col-padding">
                <div class="main-box-body">
                    <div class=" form-horizontal col-padding" style="padding-top:40px;padding-bottom:40px; ">
                        <div class="col-md-3" style="width:auto;">


                            <div style="display:inline;position:relative;">
                                <ol class="dd-list">
                                    <!-- ko template: { name: 'reportCategoriesTemplate' , foreach: reportcategoriesList } -->
                                    <!-- /ko -->

                                  
                                </ol>


                            </div>
                        </div>

                        <div style="display:inline;width:60% !important;" class="col-md-8">
                            <div class="treeviewClickCharge-partial">
                                <div class="form-group">
                                    <div class="col-lg-6 text-right">
                                        <label  class="control-label text-right tooltip-float">Title: </label>
                                    </div>
                                    <div class="col-lg-5" >
                                        <span> NA</span>
                                       
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-lg-6 text-right">
                                        <label class="control-label text-right tooltip-float">Code: </label>
                                    </div>
                                    <div class="col-lg-5">
                                        <span> NA</span>

                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-lg-6 text-right">
                                        <label class="control-label text-right tooltip-float">Name: </label>
                                    </div>
                                    <div class="col-lg-5">
                                        <span> NA</span>

                                    </div>
                                </div>
                                <div class="form-group">
                                  
                                    <div>
                                        <span> Output To:</span>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-lg-3"></div>
                                    <div class="col-lg-5 PaddingleftZero" style="width: 47px;">
                                        <input class="form-control PaddingleftZero" type="radio" value="preview" data-bind="checked: outputTo" />
                                    </div>
                                    <div class="col-lg-5">
                                        <label for="Type" class="control-label tooltip-float" style="float: left !important;">Preview</label>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-lg-3"></div>
                                    <div class="col-lg-5 PaddingleftZero" style="width: 47px;">
                                        <input class="form-control" type="radio" value="email" data-bind="checked: outputTo" />
                                    </div>
                                    <div class="col-lg-5">
                                        <label for="Type" class="control-label tooltip-float" style="float: left !important;">Email</label>
                                    </div>
                                    
                                   
                                </div>
                                <div class="form-group">
                                    <div class="col-lg-3"></div>
                                    <div class="col-lg-5 PaddingleftZero" style="width: 47px;">
                                        <input class="form-control" type="radio" value="pdf" data-bind="checked: outputTo" />
                                    </div>
                                    <div class="col-lg-5">
                                        <label for="Type" class="control-label tooltip-float" style="float: left !important;">Acrobat Reader (PDF)</label>
                                    </div>
                                   

                                </div>
                                <div class="form-group">
                                    <div class="col-lg-3"></div>
                                    <div class="col-lg-5 PaddingleftZero" style="width: 47px;">
                                        <input class="form-control" type="radio" value="excel" data-bind="checked: outputTo" />
                                    </div>
                                    <div class="col-lg-5">
                                        <label for="Type" class="control-label tooltip-float" style="float: left !important;">Microsoft Excel</label>
                                    </div>
                                    

                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" style="margin-left: 0px;">ok</button>
                <button type="button" class="btn btn-default">Cancel</button>
            </div>
        </div>
    </div>
</div>

<script id="reportCategoriesTemplate" type="text/html">
    <li class="dd-item dd-item-list" id="treeviewClickCharge">
        <div class="dd-handle-list"><i class="fa fa-bars"></i></div>
        <div class="dd-handle" data-bind="text:CategoryName">
        </div>
        <!-- ko template: { name: 'reportTemplate' , foreach: reportcategoriesList } -->
        <!-- /ko -->

    </li>

</script>

<script id="reportTemplate" type="text/html">
    <ol class="dd-list my-navigator" id="treeviewClickCharge">
        <li class="dd-item dd-item-list my-navigator">
            <div class="dd-handle-list my-navigator"><i class="fa fa-bars"></i></div>
            @*<a class="my-navigator"> <div class="dd-handle" data-bind="text:Name, click:$root.GetMachineLookupById"></div></a>*@
        </li>
    </ol>

</script>