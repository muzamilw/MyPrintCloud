<div class="modal fade" id="productFromRetailStoreModal" style="overflow-y: hidden; margin-top: -14px;" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" data-backdrop="static" aria-hidden="true">
    <div class="modal-dialog" id="productFromRetailStoreModalDialog">
        <div class="modal-content">
            <div class="modal-header" id="dialgHeaderPadding">
                <button type="button" class="close" data-dismiss="modal" data-bind="click:$root.onCloseProductFromRetailStore" aria-hidden="true">&times;</button>
                <h4 class="modal-title">Product From Store</h4>
            </div>
            <div class="col-lg-12 form-horizontal" style=" margin-top: 30px;">
                <div class="col-lg-3 center_align form-horizontal outerDiv-Padding" id="OrderDialogSection1" style="margin-left: 9px; width: 30%;">
                    @*<header class="main-box-header clearfix">
                            <div class="filter-block pull-left">
                                <form data-bind="submit: $root.getItemsByCompanyId">
                                    <div class="form-group pull-left">
                                        <input style="margin-left:16px" type="text" class="form-control" placeholder="Search..." data-bind="value: $root.searchFilter">
                                        <i class="fa fa-search search-icon" data-bind="click: $root.getItemsByCompanyId"></i>
                                    </div>
                                </form>
                            </div>
                        </header>*@
                    <h4 class="group-header">Select a Product</h4>

                    <div class="filter-body" id="filter">
                        <div class="filter-block pull-right">
                            <div class="pull-left spanfilterlbl">
                                &nbsp;
                            </div>
                            <div class="form-group pull-right">
                                <form data-bind="submit: $root.getItemsByCompanyId">
                                    <input type="text" style="width:214px" class="form-control" placeholder="Search..." data-bind="value: $root.searchFilter">
                                    <i class="fa fa-search search-icon" data-bind="click:$root.getItemsByCompanyId"></i>
                                </form>
                            </div>
                        </div>
                        <div class="clearfix">
                        </div>
                    </div>

                    <div class="main-box-body clearfix" style="max-height: 292px; min-height: 292px;overflow-y: auto">
                        <div class="table-responsive">
                            <table class="table table-striped table-hover">
                                <thead>
                                    <tr>
                                        <th class="col-lg-5">Code</th>
                                        <th class="col-lg-7">Name</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <!--ko if: $root.orderProductItems().length==0-->
                                    <tr>
                                        <td class="col-lg-6">
                                            <span>No Item found!</span>
                                        </td>
                                        <td></td>
                                    </tr>
                                    <!-- /ko -->
                                    <!-- ko template: { name: 'itemsTemplate' , foreach: $root.orderProductItems } -->
                                    <!-- /ko -->
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div style="display: none" data-bind="visible:$root.costCenterVisibility()">
                    <!-- controls visibility of other sections- starts here-->
                    <div class="col-lg-1" style=" padding-left: 3px; width: 7%;">
                        <div style="height: 300px; position: relative; top: 50%; transform: translateY(55%); padding-right: 0px; ">
                            <img src="~/Content/Images/arrow-right.jpg" style="width: 45px;" data-bind="click:$root.showCostCenter" />
                        </div>
                    </div>

                    <div style=" margin-left: -25px; height: 415px; max-height: 415px; width: 30%;" @*data-bind="visible:$root.costCenterVisibility()"*@ class="col-lg-3 center_align form-horizontal outerDiv-Padding" id="OrderDialogSection2">

                        <div class="form-group" style="margin-left: 0px; margin-right: 0px;  overflow-y: auto" data-bind="css: {'productDialogEmptyHeight': ( $root.selectedStockItem() == undefined || ($root.selecteditem() != undefined && $root.selecteditem().itemPriceMatrices().length == 0)), 'productDialogNonEmptyHeight': ($root.selectedStockItem() != undefined && ($root.selecteditem() != undefined && $root.selecteditem().itemPriceMatrices().length != 0))}">

                            <h4 class="group-header"> Select Price Attribute & Quantity</h4>
                            <div class="filter-body" id="filter">
                                <div class="filter-block pull-right">
                                    <div class="col-lg-12">
                                        <div class="form-group pull-right">
                                            <!--ko if: $root.selecteditem()!=undefined -->
                                            <select class="form-control" data-bind="options:$root.selecteditem().itemStockOptions,optionsText:'label',optionsValue:'id', value: $root.selectedStockItem, style: { marginRight: $root.quantitiesCount() <=3 ? '-9px' : '-6px', width: $root.quantitiesCount() <=3 ? '215px' : '194px' }"></select>
                                            <!--/ko-->

                                        </div>
                                    </div>
                                </div>
                                <div class="clearfix">
                                </div>
                            </div>

                            <!--ko if: $root.selecteditem()!=undefined && $root.selectedStockOption() != undefined-->
                            <div class="main-box-body clearfix">
                                <div class="table-responsive">
                                    <table class="table">
                                        <thead>
                                            <tr>
                                                <!-- ko if: $root.selecteditem().isQtyRanged() == 2 -->
                                                <th class="col-lg-5">Quantity</th>
                                                <!--/ko-->
                                                <!-- ko if: $root.selecteditem().isQtyRanged() == 1 -->
                                                <th class="col-lg-3">Quantity From</th>
                                                <th class="col-lg-3">Quantity To</th>
                                                <!--/ko-->
                                                <th style="text-align: right" class="col-lg-7 " data-bind="text: $root.selectedStockOptionName"></th>
                                                <th class="col-lg-1"></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <!--ko if: $root.selecteditem().itemPriceMatrices().length==0-->
                                            <tr>
                                                <td class="col-lg-6">
                                                    <span>No Item found!</span>
                                                </td>
                                                <td></td>`
                                                <td></td>
                                            </tr>
                                            <!-- /ko -->
                                            <!-- ko template: { name: 'itemPriceMatrixTemplate' , foreach: $root.selecteditem().itemPriceMatrices } -->
                                            <!-- /ko -->
                                        </tbody>
                                    </table>
                                </div>
                            </div>

                            <!--/ko-->
                        </div>
                        <div class="form-group col-lg-12">
                            <!-- ko if: $root.selecteditem()!=undefined && $root.selecteditem().itemPriceMatrices().length != 0  -->
                            <!--ko if: $root.selecteditem()!=undefined && $root.selectedStockOption() != undefined -->
                            <!-- ko if: $root.selecteditem().isQtyRanged() == 1 -->
                            <div class=" col-lg-3 text-right "><label class="control-label tooltip-float">Enter Quantity</label></div>
                            <div class=" col-lg-5"><input type="text" class="form-control" data-bind="value: $root.selectedProductQuanity" /></div>
                            <!-- /ko -->
                            <!-- ko if: $root.selecteditem().isQtyRanged() == 2 -->

                            <div class=" col-lg-8">
                                <select class="form-control" data-bind="options: $root.productQuantitiesList, value: $root.selectedProductQuanity, optionsCaption: 'Select Quantity'"></select>
                            </div>
                            <!-- /ko -->
                            <!-- /ko -->
                            <!--ko if: $root.selecteditem()!=undefined && $root.selectedStockOption() != undefined -->
                            <div class=" col-lg-4 text-right " data-bind="style: { paddingRight: $root.quantitiesCount() <=3 ? '3px' : '16px' }">
                                <label class="control-label tooltip-float">
                                    <span data-bind="text:$root.currencySymbol()"></span>
                                    <span data-bind="numberInput:$root.selectedProductQuanityPrice"></span>
                                </label>
                            </div>
                            <!-- /ko -->
                            <!--/ko-->
                        </div>
                    </div>
                    <div class="col-lg-1" style=" padding-left: 3px; width: 7%;">
                        <div style="height: 300px; position: relative; top: 50%; transform: translateY(55%); ">
                            <img src="~/Content/Images/arrow-right.jpg" style="width: 45px;" />
                        </div>
                    </div>

                    <div style="margin-left: -25px; height: 415px; max-height: 415px; width: 29%;" class="col-lg-3 center_align form-horizontal outerDiv-Padding" id="OrderDialogSection1" style="">
                        @*<header class="main-box-header clearfix">
                                <div class="filter-block pull-left">
                                    <form data-bind="submit: $root.getItemsByCompanyId">
                                        <div class="form-group pull-left">
                                            <input style="margin-left:16px" type="text" class="form-control" placeholder="Search..." data-bind="value: $root.searchFilter">
                                            <i class="fa fa-search search-icon" data-bind="click: $root.getItemsByCompanyId"></i>
                                        </div>
                                    </form>
                                </div>
                            </header>*@
                        <h4 class="group-header">Select Refining Options</h4>

                        <div class="main-box-body clearfix" style="max-height: 292px; min-height: 292px;overflow-y: auto">
                            <div class="table-responsive">
                                <table class="table table-striped table-hover">
                                    <thead>
                                        <tr>
                                            <th class="col-lg-1"></th>
                                            <th class="col-lg-5">Cost Center</th>
                                            <th class="col-lg-4 text-right">Total</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <!-- ko if: $root.selectedStockOption() !== undefined-->
                                        <!-- ko if: $root.selectedStockOption() !== undefined && $root.selectedStockOption().itemAddonCostCentres().length <= 0 -->
                                        <tr>
                                            <td></td>
                                            <td><span>No item found!</span></td>
                                            <td></td>
                                        </tr>
                                        <!-- /ko -->
                                        <!-- ko template: { name: 'addOnTemplate' , foreach: $root.selectedStockOption().itemAddonCostCentres } -->
                                        <!-- /ko -->
                                        <!-- /ko -->
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <!-- ko if: $root.selectedStockOption() !== undefined && $root.selectedStockOption().itemAddonCostCentres().length >= 1 -->
                        <div style="margin-top: 16px;" class="col-lg-12">
                            <label class="col-lg-7">Refining Options Total </label>
                            <span class="col-lg-3" style="text-align: right; padding-right: 0px;" data-bind="text: $root.currencySymbol"></span>
                            <span class="col-lg-2" style="text-align: right; padding-right: 0px; padding-left: 0px; " data-bind="numberInput: addsOnTotal"></span>
                        </div>
                        <!-- /ko -->

                    </div>

                </div> <!-- controls visibility of other sections- ends here-->
            </div>
            <div class="col-lg-12 form-horizontal" style="display: none" data-bind="visible:$root.costCenterVisibility()">
                <!--ko if: $root.selecteditem()!=undefined && $root.selectedStockOption() != undefined -->
                <div class="col-lg-9"></div>
                <div class="col-lg-3" style="margin-top: 10px;">
                    <label class="col-lg-8">Total Inc. Tax</label>
                    <span class="col-lg-1" style="text-align: right; padding-right: 0px" data-bind="text: $root.currencySymbol"></span>
                    <span class="col-lg-3" style=" text-align: right; padding-left: 0px;margin-left: -8px" data-bind="numberInput: totalProductPrice"></span>
                </div>
                <!-- /ko -->
            </div>
            <div class="modal-footer">
                <hr />
                <button type="button" class="btn btn-primary" style=" margin-top: -10px" data-bind="click: $root.onSaveRetailStoreProduct">Add</button>    @*data-bind="click: $root.onSaveCompanyTerritory"*@
                @*<button type="button" class="btn btn-default" data-bind="click: $root.onCloseProductFromRetailStore" style="margin-left: 0px;">Close</button>*@
            </div>
        </div>
    </div>
</div>
<script id="itemsTemplate" type="text/html">
    <tr data-bind="click : $root.updateItemsDataOnItemSelection, css: { selectedRow: $root.selectedProductFromStore() === $data }" style="cursor: pointer">
        <td data-bind="text: productCode, css: { selectedRow: $root.selectedProductFromStore() === $data }"></td>
        <td data-bind="text: productName, css: { selectedRow: $root.selectedProductFromStore() === $data }"></td>
    </tr>
</script>
<script id="addOnTemplate" type="text/html">
    <tr>
        <td style=" padding-bottom: 0px;">
            <div class="col-xs-1">
                <div class="checkbox-nice">
                    <input type="checkbox"  data-bind="checked: isSelected,attr: { id:  $index() }" />
                    <label data-bind="attr: { for:  $index() }">
                    </label>
                </div>
            </div>

        </td>
        <td data-bind="text: costCentreName"></td>
        <td style="text-align: right">
            <span style="text-align: right" data-bind="text: $root.currencySymbol"></span>
            <span style="text-align: right" data-bind="numberInput: totalPriceWithTax"></span>
        </td>
    </tr>
</script>
<script id="itemPriceMatrixTemplate" type="text/html">
    <!-- ko if: quantity()!==0  -->
    <tr>
        @*data-bind="click : $root.updateItemsDataOnItemSelection"*@
        <!-- ko if: $root.selecteditem().isQtyRanged() == 2 -->
        <td data-bind="text: quantity"></td>
        <!--/ko-->
        <!-- ko if: $root.selecteditem().isQtyRanged() == 1 -->
        <td data-bind="text:qtyRangedFrom"></td>
        <td data-bind="text: qtyRangedTo"></td>
        <!--/ko-->
        <!-- ko if: $root.selectedStockOptionSequenceNumber() == 1 -->
        <td style="text-align: right">
            <span style="text-align: right" data-bind="text: $root.currencySymbol"></span>
            <span style="text-align: right" data-bind="numberInput: pricePaperType1WithTax"></span>
        </td>
        <!--/ko-->
        <!-- ko if: $root.selectedStockOptionSequenceNumber() == 2 -->

        <td style="text-align: right">
            <span style="text-align: right" data-bind="text: $root.currencySymbol"></span>
            <span style="text-align: right" data-bind="numberInput: pricePaperType2WithTax"></span>
        </td>
        <!--/ko-->
        <!-- ko if: $root.selectedStockOptionSequenceNumber() == 3 -->
        <td style="text-align: right">
            <span style="text-align: right" data-bind="text: $root.currencySymbol"></span>
