<div class="row" data-bind="visible: isProductDetailsVisible" style="display: none">
    <div class="col-lg-12">
        <div class="row" data-bind="with: selectedProduct">
            <div class="col-lg-12">
                <div class="clearfix">
                    <div class="pull-left validationSummaryContainer">
                        <a class="validationSummaryLink" data-bind="visible: $root.errorList().length > 0, bootstrapPopover: { html: true, title: 'Please fix following errors', elementNode: 'validationSummaryPopup', popoverId: 'validationSummaryPopoverBody' }" style="display: none">Validation Errors</a>
                        <div class="well well-small validationSummaryModal" id="validationSummaryPopup" style="display: none">
                            <div id="validationSummaryPopoverBody">
                                <ul data-bind="foreach: $root.errorList">
                                    <li>
                                        <a data-bind="click: $root.gotoElement.bind($data.element)"><span data-bind="text: name"></span></a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="pull-right top-page-ui">
                        <button class="btn btn-primary" id="saveBtn" data-bind="click: $root.onSaveProduct, css: { 'btn-warning': hasChanges }, enable: hasChanges">Save</button>
                        <button class="btn btn-default" id="cancelBtn" data-bind="click: $root.onCloseProductEditor">Cancel</button>
                    </div>
                </div>
            </div>
        </div>
        <div>&nbsp;</div>
        <div class="row" id="user-profile">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="main-box clearfix my-tabsHeader">
                    <div class="tabs-wrapper profile-tabs">
                        <ul class="nav nav-tabs" data-bind="with: selectedProduct">
                            <li class="active"><a href="#tab-ProdNameAndImages" data-toggle="tab">Basic Details</a></li>
                            <li><a href="#tab-ProductDetailSection" data-toggle="tab">Detail Page and Section</a></li>
                            <li><a href="#tab-StockLabels" data-toggle="tab">Stock Labels</a></li>
                            <li><a href="#tab-pricetable" data-toggle="tab">Price Table Defaults</a></li>
                            <li><a href="#tab-supplierPrice" data-toggle="tab">Supplier Prices</a></li>
                            <li><a href="#tab-internalDescription" data-toggle="tab">Internal Descriptions</a></li>
                            <!-- ko if: isTemplateTabsVisible -->
                            <li><a href="#tab-tempProperties" id="tabTempProperties" data-toggle="tab">Template Properties</a></li>
                            <li><a href="#tab-tempPages" data-toggle="tab" data-bind="visible: template().canAddTemplatePages">Template Pages</a></li>
                            <!-- /ko -->
                            <li><a href="#tab-imageManagment" data-toggle="tab">Image Management</a></li>
                        </ul>
                        <div class="tab-content">
                            <div class="tab-pane fade in active" id="tab-ProdNameAndImages" data-bind="with: selectedProduct">
                                <div class="col-lg-12 form-horizontal">
                                    <h3 class="group-header">Name and Type</h3>
                                    <div class="col-lg-9">
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <label for="txtCompanyName" class="control-label text-right"></label>
                                            </div>
                                            <div class="col-lg-9">
                                                <div class="radio">
                                                    <input type="radio" name="optionsRadios" id="optionYesForProduct" value="1" data-bind="checked: isFinishedGoodsUi">
                                                    <label for="optionYesForProduct">
                                                        Print  Product
                                                    </label>
                                                </div>
                                                <div class="radio">
                                                    <input type="radio" name="optionsRadios" id="optionNoForProduct" value="3" data-bind="checked: isFinishedGoodsUi">
                                                    <label for="optionNoForProduct">
                                                        Non Print Product
                                                    </label>
                                                </div>
                                                <div class="radio">
                                                    <input type="radio" name="optionsRadios" id="optionYesFinishForProduct" value="2" data-bind="checked: isFinishedGoodsUi">
                                                    <label for="optionYesFinishForProduct">
                                                        Marketing brief ( User can submit an Inquiry with information)
                                                    </label>
                                                </div>

                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">

                                                    <a class="input-group-addon" style="text-decoration: none; border: 0; cursor: pointer;" border 0" data-container="body" data-toggle="popover" data-placement="right" data-content="Enter Product Name" data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>

                                                </div>
                                                <label for="txtProductName" class="control-label text-right tooltip-float">Product Name</label>

                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" class="form-control" id="txtProductName" name="Product Name" placeholder="Enter Product Name" data-bind="value: productName, validationTooltip: productName, validationOnElement: productName">
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">

                                                    <a class="input-group-addon" style="text-decoration: none; border: 0; cursor: pointer;" border 0" data-container="body" data-toggle="popover" data-placement="right" data-content="Select Product Name" data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>

                                                </div>


                                                <label for="txtProductName" class="control-label text-right tooltip-float">Product Code</label>

                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" class="form-control" id="txtProductCode" name="Product Code" placeholder="Enter Product Code" data-bind="value: productCode, validationTooltip: productCode, validationOnElement: productCode">
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">

                                                    <a class="input-group-addon" style="text-decoration: none; border: 0; cursor: pointer;" border 0" data-container="body" data-toggle="popover" data-placement="right" data-content="Display Option" data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>

                                                </div>
                                                <label for="txtCompanyName" class="control-label text-right">Display Options</label>

                                            </div>
                                            <div class="col-lg-9">
                                                <div class="radio">
                                                    <input type="radio" name="optionsRadiosDO" id="optionYesDO" value="option1" checked>
                                                    <label for="optionYesDO">
                                                        Display the Product thumbnail and Larger image.
                                                    </label>
                                                </div>
                                                <div class="radio">
                                                    <input type="radio" name="optionsRadiosDO" id="optionNoDO" value="option2">
                                                    <label for="optionNoDO">
                                                        Display the Product Banners
                                                    </label>
                                                </div>


                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">

                                                    <a class="input-group-addon" style="text-decoration: none; border: 0; cursor: pointer;" border 0" data-container="body" data-toggle="popover" data-placement="right" data-content="Sort Order in Category" data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>

                                                </div>


                                                <label for="txtSortOrder" class="control-label text-right tooltip-float">Category Sort Order</label>

                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" class="form-control" id="txtSortOrder" placeholder="Enter Sort Order" data-bind="value: sortOrder">
                                                <span class="help-block text-right">Sort Order in Category</span>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">

                                                    <a class="input-group-addon" style="text-decoration: none; border: 0; cursor: pointer;" border 0" data-container="body" data-toggle="popover" data-placement="right" data-content="Categories List" data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>

                                                </div>


                                                <label for="txtSortOrder" class="control-label text-right tooltip-float">Categories</label>

                                            </div>
                                            <div class="col-lg-6">
                                                <label class="control-label" data-bind="text: availableProductCategoryItems">
                                                </label>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">



                                                <label for="txtSortOrder" class="control-label text-right tooltip-float"></label>

                                            </div>
                                            <div class="col-lg-6" style="padding-left:0px;">
                                                <div class="col-xs-5" style="padding-left:0px;">
                                                    <a href="#" style="margin-left:4px;" data-bind="click: $root.openProductCategoryDialog">
                                                        <i class="fa fa-lg"></i>&nbsp;Select Categories
                                                    </a>
                                                </div>
                                                <div class="col-xs-1">  </div>
                                                <div class="col-xs-6">
                                                    @*<a data-toggle="modal" href="#myModalss" class=" pull-right">
                                                <i class="fa fa-lg"></i>&nbsp;Create a new Category
                                            </a>*@
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-3 col-md-4 col-sm-4">
                                        <div class=" clearfix">
                                            <header class="main-box-header clearfix">
                                                <h2 data-bind="text: productName"></h2>
                                            </header>
                                            <div class="main-box-body clearfix">
                                                <div class="profile-status">

                                                </div>
                                                <img src="" alt="" data-bind="attr: { src: thumbnail }" class="profile-img img-responsive center-block" />
                                                <div class="profile-label">
                                                    <span class="label label-danger" data-bind="text: isArchived() ? 'In-Active' : 'Active', css:{ 'label-danger': isArchived, 'label-danger': !isArchived() }">Active</span>
                                                </div>
                                                <div class="profile-since">

                                                </div>
                                                <div class="profile-details">
                                                    <ul class="fa-ul">
                                                        <li><i class="fa-li fa fa-cloud"></i>Published: <i class="fa" data-bind="css:{ 'fa-check': isPublished, 'fa-times': !isPublished() }"></i>  </li>
                                                        <li><i class="fa-li fa fa-archive"></i>Enables: <i class="fa" data-bind="css:{ 'fa-check': isEnabled, 'fa-times': !isEnabled() }"></i></li>

                                                    </ul>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-12 form-horizontal">
                                    <h3 class="group-header">Product Option</h3>
                                    <div class="col-lg-9">
                                        <div class="form-group">
                                            <div class="col-lg-7 text-right" style="margin-right:35px;">

                                                <label class="control-label text-right" for="switchSecondaryPages">Enabled</label>

                                            </div>
                                            <div class="col-lg-3">
                                                <div>
                                                    <div class="onoffswitch">
                                                        <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="switchEnableProduct" data-bind="checked: isEnabled">
                                                        <label class="onoffswitch-label" for="switchEnableProduct">
                                                            <div class="onoffswitch-inner"></div>
                                                            <div class="onoffswitch-switch"></div>
                                                        </label>
                                                    </div>
                                                    <span class="help-block text-right"></span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-7 text-right" style="margin-right:35px;">

                                                <label class="control-label text-right" for="switchSecondaryPages">Published on Store</label>

                                            </div>
                                            <div class="col-lg-3">
                                                <div class="onoffswitch">
                                                    <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="switchPublishOnWebsite" data-bind="checked: isPublished">
                                                    <label class="onoffswitch-label" for="switchPublishOnWebsite">
                                                        <div class="onoffswitch-inner"></div>
                                                        <div class="onoffswitch-switch"></div>
                                                    </label>
                                                </div>
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-7 text-right" style="margin-right:35px;">

                                                <label class="control-label text-right" for="switchFeaturedProduct">Featured Product (visible in featured products widget on home page if selected)</label>

                                            </div>
                                            <div class="col-lg-3">
                                                <div class="onoffswitch">
                                                    <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="switchFeaturedProduct" data-bind="checked: isFeatured">
                                                    <label class="onoffswitch-label" for="switchFeaturedProduct">
                                                        <div class="onoffswitch-inner"></div>
                                                        <div class="onoffswitch-switch"></div>
                                                    </label>
                                                </div>
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-7 text-right" style="margin-right:35px;">

                                                <label class="control-label text-right" for="switchSecondaryPages">This is a Digital Product, Allows Downloading of output PDF after order is placed by special Email and from Receipt page</label>

                                            </div>
                                            <div class="col-lg-3">
                                                <div class="onoffswitch">
                                                    <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="switchAllowPDF" checked>
                                                    <label class="onoffswitch-label" for="switchAllowPDF">
                                                        <div class="onoffswitch-inner"></div>
                                                        <div class="onoffswitch-switch"></div>
                                                    </label>
                                                </div>
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="col-lg-7 text-right" style="margin-right:35px;">

                                                <label class="control-label text-right" for="switchSecondaryPages">Is this Real Estate Product ( Allows Merging of Property/Asset data into Template )</label>

                                            </div>
                                            <div class="col-lg-3">
                                                <div class="onoffswitch">
                                                    <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="switchAPI" checked>
                                                    <label class="onoffswitch-label" for="switchAPI">
                                                        <div class="onoffswitch-inner"></div>
                                                        <div class="onoffswitch-switch"></div>
                                                    </label>
                                                </div>
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>


                                        <div class="form-group">
                                            <div class="col-lg-7 text-right" style="margin-right:35px;">

                                                <label class="control-label text-right" for="switchFeaturedProduct">User must upload Files or Artwork (pdf,jpg,eps,tiff,tif,csv,xls,xlsx) for this product? </label>
                                                <span class="label label-circle-questions">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="onoffswitch">
                                                    <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="switchReqFiles" checked>
                                                    <label class="onoffswitch-label" for="switchReqFiles">
                                                        <div class="onoffswitch-inner"></div>
                                                        <div class="onoffswitch-switch"></div>
                                                    </label>
                                                </div>
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-7 text-right" style="margin-right:35px;">

                                                <label class="control-label text-right" for="switchSecondaryPages">Is this a VDP product ? Apply prices as per below table</label>

                                            </div>
                                            <div class="col-lg-3">
                                                <div class="onoffswitch">
                                                    <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="switchVDPs" data-bind="checked: isVdpProduct">
                                                    <label class="onoffswitch-label" for="switchVDPs">
                                                        <div class="onoffswitch-inner"></div>
                                                        <div class="onoffswitch-switch"></div>
                                                    </label>
                                                </div>
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-7 text-right" style="margin-right:35px;">

                                                <label class="control-label text-right tooltip-float" for="switchFeaturedProduct">Enable stock level</label>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="onoffswitch">
                                                    <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="switchStockLevelProduct" data-bind="checked: isStockControl">
                                                    <label class="onoffswitch-label" for="switchStockLevelProduct">
                                                        <div class="onoffswitch-inner"></div>
                                                        <div class="onoffswitch-switch"></div>
                                                    </label>
                                                </div>
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <div class="col-lg-12 form-horizontal" id="VDPTable" data-bind="visible: isVdpProduct">
                                    <h3 class="group-header">VDP Price Table</h3>
                                    <header class="clearfix">
                                        <div class="filter-block pull-right">
                                            <a class="btn btn-primary pull-right" data-bind="click: addItemVdpPrice, enable: canAddItemVdpPrice, attr: { title: !canAddItemVdpPrice() ? 'Can not add more than 15' : '' }">
                                                <i class="fa fa-plus-circle fa-lg"></i>&nbsp;
                                            </a>
                                        </div>
                                    </header>
                                    <div>&nbsp;</div>
                                    <div class="main-box no-header clearfix">
                                        <div class="main-box-body clearfix">
                                            <div class="table-responsive">
                                                <table class="table table-striped table-hover">
                                                    <thead>
                                                        <tr>
                                                            <th style="text-align:center;"><a href="#"><span>RANGE of Clicks</span></a></th>
                                                            <th style="text-align:center;"><a href="#"><span> Price per Click</span></a></th>
                                                            <th style="text-align:center;"><a href="#"><span>Setup Charge</span></a></th>
                                                            <th>&nbsp;</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <!-- ko template: { name: 'itemVdpPriceTemplate', foreach: itemVdpPrices } -->
                                                        <!-- /ko -->
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>

                            <div class="tab-pane fade" id="tab-ProductDetailSection" data-bind="with: selectedProduct">
                                <div class="col-md-12 form-horizontal">
                                    <h3 class="group-header">Detail Page and Section</h3>
                                    <div class="col-md-9">
                                        <div class="form-group">
                                            <div class="col-lg-3 text-right">
                                                <label class="control-label text-right"></label>

                                            </div>
                                            <div class="col-lg-9" data-bind="with: itemProductDetail">
                                                <div class="radio">
                                                    <input type="radio" name="sectionOptionsRadiosPrint" id="sectionOptionPrintYes" data-bind="checked: isPrintItemUi" value="2">
                                                    <label for="sectionOptionPrintYes">
                                                        No Print - No Signatures required ( no section need to be define)
                                                    </label>
                                                </div>
                                                <div class="radio">
                                                    <input type="radio" name="sectionOptionsRadiosPrint" id="sectionOptionPrintNo" value="1" data-bind="checked: isPrintItemUi">
                                                    <label for="sectionOptionPrintNo">
                                                        Digital Litho Print Paginations
                                                    </label>
                                                </div>

                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>

                                        <div class="form-group" data-bind="visible: itemProductDetail().isPrintItemUi() === '1'">
                                            <div class="col-lg-3 text-right">


                                            </div>
                                            <div class="col-lg-9">
                                                <label style="font-weight:bold;" class="control-label text-right">Create the preffered Signatures (sections) layout for this product</label>
                                                <span class="help-block text-right"></span>
                                            </div>
                                        </div>
                                        <div class="form-group" data-bind="visible: itemProductDetail().isPrintItemUi() === '1'">
                                            <div class="col-lg-3 text-right">
                                                <div class="bs-example-tooltips tooltip-float-width">

                                                    <a class="input-group-addon" data-container="body" data-toggle="popover" data-placement="right" data-content="Unique Signatures" data-original-title="" title="">
                                                        <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                    </a>

                                                </div>



                                                <label class="control-label text-right  tooltip-float">Unique Signatures</label>
                                            </div>
                                            <div class="col-lg-9">
                                                <div class="col-xs-2">
                                                    <input type="text" disabled="disabled" style="width:95px;" class="form-control" id="txtSection" data-bind="value: itemSections().length">
                                                    <span style="width:100px;" class="help-block text-right">(up to 5 Permitted)</span>

                                                </div>
                                                <div class="col-xs-2" style="width:15%;">
                                                    <button id="AddSection" type="button" style="float:left;" data-bind="visible: canAddItemSection, click: addItemSection" class="btn btn-success">
                                                        <span class="fa"></span> +
                                                    </button>
                                                    <button id="RemoveSection" type="button" style="float:right;display:none;" data-bind="visible: canRemoveItemSection, click: removeItemSection" class="btn btn-danger">
                                                        <span class="fa"></span>
                                                        -
                                                    </button>
                                                </div>
                                                <div class="col-xs-4">
                                                </div>
                                                <div class="col-xs-4">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-9" style="margin-bottom:10px; margin-left:19%;" data-bind="visible: itemProductDetail().isPrintItemUi() === '1'">
                                        <div id="mainDivSections">
                                            <!-- ko foreach: itemSections -->
                                            <div class="col-xs-3">
                                                <div class="col-xs-8">
                                                    <input type="text" id="detailSectionName" class="form-control" data-bind="value: name, validationToolTip: name, validationOnElement: name">
                                                </div>
                                                <div class="col-xs-4">
                                                    <a href="#" data-bind="click: $root.editSectionSignature" class="btn btn-primary pull-right">
                                                        <i class="fa fa-lg"></i>&nbsp;....
                                                    </a>
                                                </div>
                                            </div>
                                            <!-- /ko -->
                                        </div>
                                    </div>



